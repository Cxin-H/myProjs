/**
 * Created by Summer Yang (summer.yang@d1m.cn) on 2015/8/3.
 * All push event of GTM
 *
 * variable dataLayer should already be defined in the header of page
 */
var gtmMgr = {
  eventName: 'trackEvent',
  event: {
    initInfo: function(initInfo) {
      window.dataLayer.push(initInfo);
    },
    cartClick: function(eventCategory,eventAction,eventLabel,eventValue){
      window.dataLayer.push({
          'event': gtmMgr.eventName,
          'eventCategory': eventCategory,
          'eventAction': eventAction,
          'eventLabel': eventLabel,
          'eventValue': eventValue
      });
    },
    loginClick: function(eventCategory,eventAction,eventLabel,eventValue){
      window.dataLayer.push({
          'event': gtmMgr.eventName,
          'eventCategory': eventCategory,
          'eventAction': eventAction,
          'eventLabel': eventLabel,
          'eventValue': eventValue
      });
    },
    registerClick: function(eventCategory,eventAction,eventLabel,eventValue){
      window.dataLayer.push({
          'event': gtmMgr.eventName,
          'eventCategory': eventCategory,
          'eventAction': eventAction,
          'eventLabel': eventLabel,
          'eventValue': eventValue
      });
    },
    checkoutLoginClick: function(eventCategory,eventAction,eventLabel,eventValue){
      window.dataLayer.push({
          'event': gtmMgr.eventName,
          'eventCategory': eventCategory,
          'eventAction': eventAction,
          'eventLabel': eventLabel,
          'eventValue': eventValue
      });
    },
    accountClick: function(eventCategory,eventAction,eventLabel,eventValue){
      window.dataLayer.push({
          'event': gtmMgr.eventName,
          'eventCategory': eventCategory,
          'eventAction': eventAction,
          'eventLabel': eventLabel,
          'eventValue': eventValue
      });
    },
    checkoutClick: function(eventCategory,eventAction,eventLabel,eventValue){
      window.dataLayer.push({
          'event': gtmMgr.eventName,
          'eventCategory': eventCategory,
          'eventAction': eventAction,
          'eventLabel': eventLabel,
          'eventValue': eventValue
      });
    },
    orderViewClick: function(eventCategory,eventAction,eventLabel,eventValue){
      window.dataLayer.push({
          'event': gtmMgr.eventName,
          'eventCategory': eventCategory,
          'eventAction': eventAction,
          'eventLabel': eventLabel,
          'eventValue': eventValue
      });
    },
    checkoutMethodStep: function(option) {
      window.dataLayer.push({
          'event' : 'tpv',
          'pageVirtualPath' : '/beauty/zh_cn/store/checkout/' + option
      });
    },
    checkoutStep: function(stepNum, option) {
      window.dataLayer.push({
        'event': 'checkoutOption',
        'ecommerce': {
          'checkout': {
            'actionField': {'step': stepNum, 'option': option}
          }
        }
      });
    }
  }, //end events
  init: function(){
    /** gtm触发点击事件**/
    /* shopping cart page */
    $j('.gtm_continue_shopping').on('click',function(){
      gtmMgr.event.cartClick('DiorCom_PCD_ShoppingCart','continue_shopping');
    });

    $j('.gtm_complete_cart').on('click',function(){
      gtmMgr.event.cartClick('DiorCom_PCD_ShoppingCart','complete_order');
    });

    $j('.gtm_giftmessage_write').on('click',function(){
      gtmMgr.event.cartClick('DiorCom_PCD_ShoppingCart','submit_giftmessage');
    });

    $j('.gtm_change_QTY').on('click',function(){
      gtmMgr.event.cartClick('DiorCom_PCD_ShoppingCart','change_qty');
    });

    $j('.gtm_delete_SKU_Cart').on('click',function(){
      var productSku = $j(this).attr('data-value');
      gtmMgr.event.cartClick('DiorCom_PCD_ShoppingCart','remove_item',productSku);
    });

    $j('.gtm_submit_promocode').on('click',function(){
      gtmMgr.event.cartClick('DiorCom_PCD_ShoppingCart','enter_promocode');
    });

    /* login page */
    //name: Registration_loginpage
    $j('#login-form .gtm_register_btn').on('click', function(){
      gtmMgr.event.loginClick('DiorCom_PCD_LoginPage', 'register');
    });

    $j('#login-form .gtm_login_btn').on('click',function(){
      gtmMgr.event.loginClick('DiorCom_PCD_LoginPage','login');
    });

    $j('#login-form .gtm_forgot_password').on('click',function(){
      gtmMgr.event.loginClick('DiorCom_PCD_LoginPage','forgot_password');
    });

    $j('.gtm_social_login').on('click',function(){
        var socialName = $j(this).attr('data-value');
      gtmMgr.event.loginClick('DiorCom_PCD_LoginPage','social_login',socialName);
    });

    /* register page */
    $j('.account-create .gtm_submit_registerpage').on('click',function(){
      gtmMgr.event.registerClick('DiorCom_PCD_RegisterPage','submit');
    });

    $j('.account-create .gtm_sms_registerpage').on('click',function(){
      gtmMgr.event.registerClick('DiorCom_PCD_RegisterPage','send_SMS_code');
    });

    /* checkout login page */
    $j('#checkout-step-login .gtm_Registration_Shopping').on('click',function(){
      gtmMgr.event.checkoutLoginClick('DiorCom_PCD_Purchase_LoginPage','register');
    });

    $j('#checkout-step-login .gtm_Guest_shopping').on('click',function(){
      gtmMgr.event.checkoutLoginClick('DiorCom_PCD_Purchase_LoginPage','continue_guest');
    });

    $j('#checkout-step-login .gtm_Login_shopping').on('click',function(){
      gtmMgr.event.checkoutLoginClick('DiorCom_PCD_Purchase_LoginPage','login');
    });

    $j('#checkout-step-login .gtm_forgotpassword_shopping').on('click',function(){
      gtmMgr.event.checkoutLoginClick('DiorCom_PCD_Purchase_LoginPage','forgot_password');
    });

    $j('#checkout-step-login .gtm_social_login_shopping').on('click',function(){
      var socialName = $j(this).attr('data-value');
      gtmMgr.event.checkoutLoginClick('DiorCom_PCD_Purchase_LoginPage','social_login',socialName);
    });

    /* checkout page */
    $j('#checkoutSteps .gtm_shippinginfo_complete').on('click',function(){
      gtmMgr.event.checkoutClick('DiorCom_PCD_ShippingInfoPage','continue');
    });

    $j('#checkoutSteps .gtm_paymentinfo_complete').on('click',function(){
      gtmMgr.event.checkoutClick('DiorCom_PCD_PaymentPage','submit');
    });

    /* my account page */
    $j('.block-account .gtm_orders').on('click',function(){
      gtmMgr.event.accountClick('DiorCom_PCD_MyAccountPage','menu','order_history');
    });

    $j('.block-account .gtm_account_edit').on('click',function(){
      gtmMgr.event.accountClick('DiorCom_PCD_MyAccountPage','menu','personal_info');
    });

    $j('.block-account .gtm_address_book').on('click',function(){
      gtmMgr.event.accountClick('DiorCom_PCD_MyAccountPage','menu','address_book');
    });

    $j('.block-account .gtm_newsletter').on('click',function(){
      gtmMgr.event.accountClick('DiorCom_PCD_MyAccountPage','menu','newsletter');
    });

    /* order view page */
    $j('.order-detail .gtm_print_order').on('click',function(){
      gtmMgr.event.orderViewClick('DiorCom_PCD_MyAccount_OrderDetailPage','reorder','print order');
    });

    $j('.order-details .gtm_reorder_btn').on('click',function(){
        $j('#my-orders-table .checkbox-product a.jqTransformChecked').each(function(i,e){
            var productSku = $j(this).parent().next().val();
            gtmMgr.event.orderViewClick('DiorCom_PCD_MyAccount_OrderDetailPage','reorder',productSku);
        });
    });

    $j('.order-details .gtm_mobile_reorder_btn').on('click',function(){
        gtmMgr.event.orderViewClick('DiorCom_PCD_MyAccount_OrderDetailPage','reorder');
    });
  }
};

var _hmt = _hmt||[];
(function() {
    var a = document.createElement("script");
    a.src = "//hm.baidu.com/hm.js?c829e1d063d54a66fa2d90b550b6b5ab";
    var b = document.getElementsByTagName("script")[0];
    b.parentNode.insertBefore(a, b);
})();
