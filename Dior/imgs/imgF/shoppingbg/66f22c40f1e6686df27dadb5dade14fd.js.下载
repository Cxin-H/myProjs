    var addressId = {};//deliveryDateByAddress
    var deliveryDateByAddressUrl = '/beauty/zh_cn/store/delivery/index/deliveryDateByAddress';


    /* my account hidden input get selected address */
    $j('.custom-select select').each(function(index,elem){
        var address = $j(this).find('option:selected').text();
        var regionsval = '';
        var regionsval1 = '';
        if(address){
            $j(this).prev().text(address);
        }

        $j(this).change(function(){
            var regions = $j(this).find('option:selected').text();
            if(regions =='选择省份' || regions =='选择城市' || regions =='选择地区'){
                $j('.delivery-time').removeClass('on');
                $j('.delivery-time').text('可选送货时间');
                $j(this).addClass('validation-failed');
            }
            else if(regions !='选择省份' || regions =='选择城市' || regions =='选择地区'){
                $j(this).removeClass('validation-failed');
            }
            // 1.[deliveryDateByAddress]没有默认值或者新增地址 change时取省市id
            if($j(this).attr('id') == 'shipping:city_id' && $j('.shipping_region_id option:selected').text() != '选择省份' && $j('.shipping_city_id option:selected').text() != '选择城市'){
                regionsval = $j('.shipping_region_id option:selected').val();
                regionsval1 = $j(this).find('option:selected').val();
                addressId = {
                    'region_id':regionsval,
                    'city_id':regionsval1
                }
//                deliveryDateByAddress(addressId);
            }
            $j(this).next().val(regions);
            $j(this).prev().text(regions);
            if($j(this).val() == ''){
                $j(this).parent().addClass('validation-failed');
            }else{
                $j(this).parent().removeClass('validation-failed');
            }
        })
    });

    function deliveryDateByAddress(o) {
        $j.ajax({
            url: deliveryDateByAddressUrl,
            type: "POST",
            dataType: 'json',
            data: o,
            beforeSend:function () {
                $j('.delivery-time').removeClass('on');
                $j('.delivery-time').text('可选送货时间');
            },
            success: function(data) {
                if(data.status){
                    $j('.delivery-time').addClass('on');
                    currentDay1 = data.dateForm;
                    currentDay2 = data.dateTo;
                    dateForm = currentDay1.replace(/-/g,"");
                    dateTo = currentDay2.replace(/-/g,"");
                    dateDiff = data.dateDiff;
                    showCalendarData(dateForm,dateTo);
                    //送货时间 弹窗
                    $j('.delivery-time').click(function () {
                        // dateDiff
                        var _td = $j('#calendar td');
                        _td.removeClass('currentTime');
                        openDeliverytime("#calendar",'600','400',currentDay1,currentDay2,dateDiff)

                    });
                }else {
                    $j('.delivery-time').removeClass('on');
                    $j('.delivery-time').text('可选送货时间');
                }
            }
        });
    }

    $j('.my-account .buttons-set button').click(function(){
        $j('.customer_address_field .custom-select select').each(function(){
            var def_region = $j(this).find('option:selected').val();
            if(!def_region){
                $j(this).parent().addClass('validation-failed');
            }else{
                $j(this).parent().removeClass('validation-failed');
            }
        });
        $j('#form-validate .form-list input.required-entry').each(function(){
            var def_val = $j(this).val();
            if(!def_val){
                $j(this).addClass('validation-failed');
                $j(this).parent().addClass('validation-failed');
            }else{
                $j(this).removeClass('validation-failed');
                $j(this).parent().removeClass('validation-failed');
            }
        });
    });

    $j('.same_as_subscriber_mobile_span .same_as_subscriber_mobile').click(function () {
        var subscriber_mobile = $j('#subscriber_mobile').val();
        if(subscriber_mobile && !$j('#subscriber_mobile').hasClass('validation-failed')){
            if($j(this).is(':checked') && subscriber_mobile) {
                $j('#shipping\\:telephone').val(subscriber_mobile);
            }else{
                $j('#shipping\\:telephone').val('');
            }
        }else{
            alert('请填写正确的订购人手机号！');
        }
    });

    //header nav selected
    var subUniverseCookie = getCookie('currentUniverse');
    $j('#dior-header nav.main-nav li a').each(function () {
        var subUniverse       = $j(this).attr('data-track-object'),
            subUniverseObj    = eval("(" + subUniverse + ")"),
            subUniverseLow    = subUniverseObj.eventLabel.toUpperCase();

        if(subUniverseLow == subUniverseCookie){
            $j('#dior-header nav.main-nav li a').removeClass('nav-item--active');
            $j(this).addClass('nav-item--active');
        }
    });

    /**文字过滤
     * @param {String} 待处理字符串
     * @param {String} 正则类型【"blankSpace":过滤空格',"defult":'默认'】
     * */
    function GetNoEmoji(strValue,regType){
        if (strValue != null && strValue != "") {
            var reg = /[\u4e00-\u9fa5a-zA-Z0-9]/g;
            var regex = /([\u4e00-\u9fa5a-zA-Z0-9\@\.\。\!\！\,\，\(\)\（\）\-\_\s])+/ig;
            var _reg;
            if(regType == 'allow-name-reg'){
                _reg = /([\u4e00-\u9fa5a-zA-Z\s])+/ig
            }else if(regType == 'allow-number'){
                _reg = /[0-9]/g
            }else if(regType == 'blankSpace'){
                _reg = reg
            }else if(regType == 'liuyan'){
                _reg = /([\u4e00-\u9fa5a-zA-Z0-9\.\。\!\！\,\，\-\_\s])+/ig
            }else if(regType == 'blockInputStreet'){
                _reg = /([\u4e00-\u9fa5a-zA-Z0-9\#\s])+/ig
            }else{
                _reg = regex
            }
            if (strValue.match(_reg)){
                return strValue.match(_reg).join("");
            }
            return "";
        } else{
            return "";
        }
    }
    // 所以input框只能输入汉字 字符 数字
    function editKezi(_this) {
        if (cpFlag) {return;}
        var val = $j(_this).val(),
            liuyantextarry = [],
            liuyansizearry =[],
            liuyansize = 0,
            regType;
        if($j(_this).hasClass('allow-name-reg')){
            regType = 'allow-name-reg'
        }else if($j(_this).hasClass('allow-number')){
            regType = 'allow-number'
        }else if($j(_this).hasClass("kezi-textarea")){
            regType = 'blankSpace'
        }else if($j(_this).hasClass("liuyan-input")){
            regType = 'liuyan'
        }else if($j(_this).hasClass("block-input-street")){
            regType = 'blockInputStreet'
        }else{
            regType = 'defult'
        }

        var text = GetNoEmoji(val,regType);
        $j(_this).val(text);
        if($j(_this).hasClass('kezi-textarea')){
            var len = $j(_this).attr('maxlength');
            $j(_this).siblings('span').find('b').text(len-text.length);
        }
        if($j(_this).hasClass('liuyan-input')){
            var remain_chr = $j(_this).parent().data('remain-chr');
            var len3 = remain_chr;
            var len2 = remain_chr/6;
            var len1 = text.length;
            if(len1 > len2 || len1 == len2){
                $j(_this).next().focus();
            }else if(len1 == 0) {
                if($j(_this).hasClass('first')){
                    $j(_this).focus();
                    $j(_this).prev().blur();
                }else {
                    $j(_this).blur();
                    $j(_this).prev().focus();
                }
            }
          $j(_this).parent().find('.liuyan-input').each(function (i,k) {
                liuyantextarry[i] = $j(k).val();
                liuyansizearry[i] = $j(k).val().length;
            });
            //字数
            $j.each(liuyansizearry, function(k,i) {
                liuyansize += i;
            });
            $j(_this).parents('.gifting-container').find('.giftbox').attr('data-liuyan',JSON.stringify(liuyantextarry));
            $j(_this).parents('.liuyan').find('.remain_chr').text(len3-liuyansize);
        }
    }
    var cpFlag = false,inputDomKezi=null;
    $j(document).on('propertychange input', 'input[type="text"]', {}, function () {
        inputDomKezi = this
        editKezi(inputDomKezi);
    }).on('compositionstart', function(){
        cpFlag=true;
    }).on('compositionend', function(){
        cpFlag=false;
        editKezi(inputDomKezi);
    });


// 快速登录
    var tab = $j('.customer-account-login .tab-login');
    var tabP = tab.parents('.registered-users');
    var tabText = $j('.customer-account-login .tab-text');
    tab.click(function () {
        if(tabP.hasClass('registered-users-login')){
            tabP.removeClass('registered-users-login');
            tab.text('密码登录');
          tabText.text('手机验证码登录');
        }else {
            tabP.addClass('registered-users-login');
            tab.text('手机验证码登录');
          tabText.text('密码登录');
        }
    });
    var q = $j('.quick-login-block-btn');
    $j(q).click(function () {
        var quickLoginUrl = '/beauty/zh_cn/store/customer/account/quicklogin';
        var quick_login_block = $j('.quick-login-block');
        var statementQ = $j(quick_login_block).find('#statement-checkbox-quick');
        if(quickForm.validator && quickForm.validator.validate() && statementQ.is(':checked')){
            $j('.quick-login-block').find('.statement-failed').fadeOut();
            if(!q.hasClass('disable')){
                $j.ajax({
                    url: quickLoginUrl,
                    method: 'POST',
                    dataType: 'json',
                    data: {'username':$j(quick_login_block).find('#quick-telephoneNo').val(),'validCode':$j('#quick-verifyCode').val(),'form_key':$j("input[name='form_key']").val()},
                    beforeSend:function () {
                        q.addClass('disable');
                        // q.parents('.buttons-set').siblings('.loading-block').show();
                    },
                    success: function (data) {
                        q.removeClass('disable');
                        // q.parents('.buttons-set').siblings('.loading-block').hide();
                        if (data.status) {
                            if(data.redirect != '' && data.redirect != undefined){
                                window.location.href = data.redirect;
                            }else {
                                var successUrl = $j('#quick-login-content').attr('data-successUrl');
                                window.location.href= successUrl;
                            }
                            if(data.new){
                                //新用户
                                //GWD 追踪数据
                                _gsq.push(['GWD-005670','trackEvent','Create an account','Message verification',registerlink]);
                            }else {
                                //老用户
                                //GWD 追踪数据
                                _gsq.push(['GWD-005670','trackEvent','Log in','Message verification Log in',registerlink]);
                            }
                        } else {
                            var errorMessage = '<ul class="messages"><li class="error-msg"><ul><li><span>'+data.message+'</span></li></ul></li></ul>';
                            $j('.register-message-box').html(errorMessage);
                        }
                    }
                });
            }
        }else {
            if(!statementQ.is(':checked')){
                $j('.quick-login-block').find('.statement-failed').fadeIn();
            }else {
                $j('.quick-login-block').find('.statement-failed').fadeOut();
            }
        }

    });

    /* 结算页面小图标提示 */
    $j(document).on('click','.tooltip-info .tooltip-btn',function () {
        $j('.tooltip-info').removeClass('active');
        $j(this).parent().toggleClass('active');
    })
    $j(document).on('click','.tooltip-info .tooltip-close',function () {
        $j(this).parents('.tooltip-info').removeClass('active');
    })

});

//获取cookie中保存的值
function getCookie(cookieName){
    var getC = document.cookie,reg = new RegExp(cookieName+'=([^;]*)');
    var val = '';
    if(reg.test(getC)){
        val = RegExp.$1;
    }
    return val;
}

//begin 顺丰可选送货时间
// var stringservertime="2018-05-10 14:00:44",
var stringservertime=new Date().getFullYear()+"-"+(new Date().getMonth()+1)+"-"+new Date().getDate()+" "+new Date().getHours()+":"+new Date().getMinutes()+":"+new Date().getSeconds(),
    servertime = new Date(Date.parse(stringservertime.replace(/-/g,"/")));

//个人信息收集
var allowClauseUrl = '/beauty/zh_cn/store/customer/account/allowClause';
function allowClauseAjax (data)
{
    $j.ajax({
        url: allowClauseUrl,
        type: "POST",
        dataType: 'json',
        data: data,
        beforeSend:function () {

        },
        success: function(data) {
            if(data.status){
                if(data.redirect_url == false){
                    $j.fancybox.close();
                }else {
                    $j.fancybox.close();
                    window.location.href = data.redirect_url

                }
            }else {
                alert(data.message)
            }
        }
    });
}
function initFancyBox(){
    var statementForm = new VarienForm('statement-form', true);
    var t = $j("#statement_popup_box_show");
    jQuery.fancybox.open({
        href:"#statement_popup_box_show",
        autoSize: false,
        fitToView: false,
        width: '600px',
        height: 'auto',
        openEffect : 'fade',
        closeEffect : 'fade',
        centerOnScroll:false,
        openSpeed: 300,
        closeBtn: true,
        closeSpeed: 0,
        overlayShow:true,
        padding:0,
        helpers: {
            overlay : {
                closeClick: false,
                locked : false,
                opacity : 0.55,
                speedIn : 300,
                speedOut : 0
            }
        },
        afterShow: function () {
            //点击条款展示内容
            $j('.show-text-block').click(function () {
                $j('.statement_content').hide();
                var num = $j(this).attr('data-num');
                $j('body').addClass('statementPopup');
                $j('.statement_content'+num).show();
                jQuery.fancybox.update();
            });
            //点击条款确认按钮
            $j('.statement-btn-yes').click(function () {

                if(statementForm.validator.validate() && $j(t).find(".jqTransformChecked").length>0){
                    var data = {
                        "allowClause":"1",
                        "form_key":$(t).find('input[name="form_key"]').val()
                    }
                    allowClauseAjax(data);


                }

            });
            //点击条款取消按钮
            $j('.statement-btn-no').click(function () {
                var data = {
                    "allowClause":"0",
                    "form_key":$(t).find('input[name="form_key"]').val()
                }
                allowClauseAjax(data);

            });
            $j('.fancybox-close').click(function () {
                var data = {
                    "allowClause":"0",
                    "form_key":$(t).find('input[name="form_key"]').val()
                }
                allowClauseAjax(data);
            })
        }
    });
}
function FancyBoxCdcAJ() {
    jQuery.fancybox.open({
        href:"#popup_cdcAJ",
        autoSize: false,
        fitToView: false,
        width: '440px',
        height: '568px',
        openEffect : 'fade',
        closeEffect : 'fade',
        centerOnScroll:false,
        openSpeed: 300,
        closeBtn: true,
        closeSpeed: 0,
        overlayShow:true,
        padding:0,
        helpers: {
            overlay : {
                closeClick: true,
                locked : true,
                opacity : 0.55,
                speedIn : 300,
                speedOut : 0
            }
        },
        afterShow: function () {
        }
    });
}
var gtmBlock = {
    eventName: 'trackEvent',
    event:{
        popUpShow: function () {
            window.dataLayer.push({
                "event": "trackEvent",
                "eventCategory":"DiorCom_PCD_MyAccount",
                "eventAction":"product list view",
                "eventLabel": "pop_up_show"
            })
        },
        popUpClose: function () {
            window.dataLayer.push({
                "event": "trackEvent",
                "eventCategory":"DiorCom_PCD_MyAccount",
                "eventAction":"product list view",
                "eventLabel": "pop_up_close"
            })
        },
        popUpClick: function () {
            window.dataLayer.push({
                "event": "trackEvent",
                "eventCategory":"DiorCom_PCD_MyAccount",
                "eventAction":"product list view",
                "eventLabel": "pop_up_click"
            })
        },
        eventBanner: function (category,label) {
            window.dataLayer.push({
                "event": "trackEvent",
                "eventCategory":"DiorCom_PCD_"+category,
                "eventAction":"product list view",
                "eventLabel": label
            })
        }
    }
}
function eventBannerGtm() {
    var category = $j('body').attr('class');
    var reg = new RegExp("-",'g');
    var _trim = $j.trim(category);
    var arry = _trim.split(/\s+/);
    var re = arry[0].replace(reg, "_");
    $j('.header-top-layer a').click(function () {
        var label = 'TopEventBannerClick';
        gtmBlock.event.eventBanner(re,label)
    });
    $j('.header-top-layer .top-layer-close').click(function () {
        var label = 'TopEventBannerClose';
        gtmBlock.event.eventBanner(re,label)
    });
    // 成功页面轮播kv 上报事件写在静态快中
}
$j(function ($) {
    //    三方登录
    $('.social-login .weChat a').click(function () {
//GWD 追踪数据
        if(typeof _gsq != 'undefined') {
            _gsq.push(['GWD-005670', 'trackEvent', 'Log in', 'Wechat Log in', registerlink]);
        }
    });
    $('.social-login .alipay a').click(function () {
//GWD 追踪数据
        if(typeof _gsq != 'undefined') {
            _gsq.push(['GWD-005670', 'trackEvent', 'Log in', 'Alipay Log in', registerlink]);
        }
    });
//  注册按钮
    $('#register-btn').click(function () {
        //GWD 追踪数据
        if(typeof _gsq != 'undefined') {
            _gsq.push(['GWD-005670', 'trackEvent', 'Register', 'Register', registerlink]);
        }
    });

//  密码登录按钮
    $('.customer-account-login #send2').click(function () {
        //GWD 追踪数据
        if(typeof _gsq != 'undefined') {
            _gsq.push(['GWD-005670', 'trackEvent', 'Log in', 'Password Log in', registerlink]);
        }
    });

//    event banner 点击
    $j('.header-top-layer .top-layer-content a').click(function () {
        //GWD 追踪数据
        var theme = $j(this).children('span').text();
        if(typeof _gsq != 'undefined') {
            _gsq.push(['GWD-005670', 'trackEvent', 'Top banner', 'Enter', theme, 0, {'1': registerlink}]);
        }
    });
//    event banner 关闭
    $j('.header-top-layer .top-layer-content .top-layer-close').click(function () {
        //GWD 追踪数据
        var theme = $j('.header-top-layer .top-layer-content a span').text();
        if(typeof _gsq != 'undefined') {
            _gsq.push(['GWD-005670', 'trackEvent', 'Top banner', 'Close', theme, 0, {'1': registerlink}]);
        }
    });
    eventBannerGtm();
    // 设置calendar div中的html部分
    renderHtml();

    //    用户中心页面判断是否弹个人信息收集框
    if($j(".customer-account-index #statement_popup_box_show").length>0){
        setTimeout(function(){
            initFancyBox();
        }, 1);

    }
    //    结算页面判断是否弹个人信息收集框
    if($j(".checkout-onepage-index #statement_popup_box_show").length>0){
        setTimeout(function(){
            initFancyBox();
        }, 1);
    }

    //成功页面是否弹aj弹窗
    if($j(".checkout-onepage-success #popup_cdcAJ").length>0){
        setTimeout(function(){
            FancyBoxCdcAJ();
        }, 1);
    }

    if($j('.customer-account-index').length>0&& $j('#messagesBlock').val() == 1) {
        gtmBlock.event.popUpShow();
        var welcomeRule = $j('#welcomeRule');
        $j('body').addClass('welcomeRule-overlay');
        $j('.welcomeRule-overlay-fixed').show();
        welcomeRule.show();
        $j(".welcomeRule-close").click(function () {
            gtmBlock.event.popUpClose();
            $j('.welcomeRule-overlay-fixed').hide();
            welcomeRule.hide();
            $j('body').removeClass('welcomeRule-overlay');
        });
        $j(document).mouseup(function(e) {
            if(!welcomeRule.is(e.target) && welcomeRule.has(e.target).length === 0 && welcomeRule.css('display') == 'block') {
                gtmBlock.event.popUpClose();
                $j('.welcomeRule-overlay-fixed').hide();
                $j('#welcomeRule').hide();
                $j('body').removeClass('welcomeRule-overlay');
            }
        });
        $j('.welcomeRuleBtn').click(function () {
            gtmBlock.event.popUpClick();
            window.location.href='/beauty/zh_cn/store/checkout/cart?DIORGIFT';
        });
    }
});


/*
 * 用于记录日期，显示的时候，根据dateObj中的日期的年月显示
 */
var dateObj = (function(){
    var _date = servertime;    // 默认为当前系统时间
    return {
        getDate : function(){
            return _date;
        },
        setDate : function(date) {
            _date = date;
        }
    };
})();
/**
 * 渲染html结构
 */
function renderHtml($) {
    // 设置表格区的html结构
    var _headHtml = "<tr class='week'>" +
        "<th>周日</th>" +
        "<th>周一</th>" +
        "<th>周二</th>" +
        "<th>周三</th>" +
        "<th>周四</th>" +
        "<th>周五</th>" +
        "<th>周六</th>" +
        "</tr>";

    var _bodyHtml = "";
    // 一个月最多31天，所以一个月最多占6行表格
    for(var i = 0; i < 6; i++) {
        _bodyHtml += "<tr>" +
            "<td data-num='"+i+"'><span></span></td>" +
            "<td><span></span></td>" +
            "<td><span></span></td>" +
            "<td><span></span></td>" +
            "<td><span></span></td>" +
            "<td><span></span></td>" +
            "<td><span></span></td>" +
            "</tr>";
    }
    // 添加到calendar div中
    $j("#calendar").html(
        "<div class='calendar-title-box'><span class='calendar-title' id='calendarTitle'></span></div><div class='calendar-body-box'><table id='calendarTable' class='calendar-table'>" +
        _headHtml + _bodyHtml +
        "</table></div><div class='delivery-hours'><span>09:00-12:00</span><span>12:01-18:00</span><span>18:01-21:00</span></div><div class='time-btn'>确认</div>"
    );
    // showCalendarData();
}
/**
 * 表格中显示数据，并设置类名
 */
function showCalendarData(time1,time) {
    var _year = time1.substr(0, 4);
    var _month = time1.substr(4,2);
    var _dateStr = time;
    // 设置顶部标题栏中的 年、月信息
    var calendarTitle = $j("#calendarTitle");
    var titleStr = _dateStr.substr(0, 4) + "年" + _dateStr.substr(4,2) + "月";
    calendarTitle.text(titleStr);
    // 设置表格中的日期数据
    var _tds = $j("#calendarTable td");
    var _tdspan = $j("#calendarTable td span");
    var _firstDay = new Date(_year, _month - 1, 1);  // 当前月第一天
    for(var i = 0; i < _tds.length; i++) {
        var _thisDay = new Date(_year, _month - 1, i + 1 - _firstDay.getDay());
        var _thisDayStr = getDateStr(_thisDay);
        //日期数据
        $j(_tdspan).eq(i).text(_thisDay.getDate());
        //每个日期的data属性
        $j(_tds).eq(i).attr("data",_thisDayStr);
    }
}
/**
 * 日期转化为字符串， 4位年+2位月+2位日
 */
function getDateStr(date) {
    var _year = date.getFullYear();
    var _month = date.getMonth() + 1;    // 月从0开始计数
    var _d = date.getDate();
    _month = (_month > 9) ? ("" + _month) : ("0" + _month);
    _d = (_d > 9) ? ("" + _d) : ("0" + _d);
    return _year + _month + _d;
}
