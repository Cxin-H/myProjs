function selectInputInit($){
  var dropdownItem =  $('*[data="data-drop"]');
  var dropDown = dropdownItem.dropdown();
  $.each(dropDown, function (i, o) {
      var dropDownDataVal =  $(o.opt.dropElem, o.elem).find('.delivery-address-new').attr('drop-down-data-val');
      if(dropDownDataVal != ''){
          $(o.elem).find('.sub-menu li a').each(function(index,ele){
              var dropdownVal= $(ele).attr('data-value'),
                  dropdownText = $(ele).text();
              if(dropDownDataVal == dropdownVal){
                  //$(o.opt.dropElem, o.elem).find('span').text($(this).text());
                  $(this).find('a').addClass('selected');
                  $(this).parents('.drop-down').find('.btn input').prop('value', dropdownVal);
              }else{

              }
          });
      }
    $(o.opt.showElem, o.elem).find('li').bind('click', function () {
      var flag = true;
      if($(this).data('address-check') < 0){
        var addressText = $(this).find('.selectitem.selected').text();
        $(this).parents('.drop-down').find('.delivery-address-new').text(addressText);
        $(this).parents('.drop-down').removeClass('open');
        $(this).parents('.sub-menu').hide();
        return false
      }
      if($(this).data('address-check') == 0){
        flag = false
      }
      if(flag){
        var name = $(this).find('.name').text();
        var address = $j(this).find('.address').text();
        var telephone = $j(this).find('.telephone').text();
        var addressText = name+','+address+','+telephone;
        $(this).siblings().find('a').removeClass('selected');
        $(this).find('a').addClass('selected');
        var selectedVal = $(this).find('a').attr("data-value") ? $(this).find('a').attr("data-value"):$(this).text();
        $(this).parents('.drop-down').find('.delivery-address-new').text(addressText);
        $(this).parents('.drop-down').find('.btn input').prop('value', selectedVal);
      }else{
        alert('非常抱歉，您的Dior迪奥时装订单无法配送到此区域。')
        return false
      }

    });
  });

}

function zhenWoblockTextarea($){
    var hxc_area = $(".block-input-street");
    var l = hxc_area.length;
    hxc_area.each(function(){
        var _this = $(this);
        var hxc_area_val = _this.val();
        var hxcB = _this.siblings("span").find("b");
        var hxc_max = _this.attr("maxlength");

        _this.bind("input propertychange", function() {//为文本输入框绑定事件
            var hxc_val = $(this).val();//获取input的值
            var b_count = zwpd(hxc_val);//获取函数zwpd的值
            if (b_count == 0) {//如果input没有内容，则输出为0
                hxcB.text(0);
            } else if (b_count <= hxc_max) {//如果input的字符数小于等于最大限制字符数，则正常输出值
                hxcB.text(b_count);
            } else {//如果input的字符数大于最大值，则最大限制字符数打印出，内容仅取textarea中最大字符数对应的内容
                hxcB.text(hxc_max);
                $(this).val(max_content(hxc_val, hxc_max));
            };
        });
    });

    function zwpd(val) {//全角字符判断函数
        var hxc_len = 0;//声明变量并赋值
        var qjreg = /[^\x00-\xff]/ig;//声明全角正则表达式
        for (i = 0; i < val.length; i++) {//建立for循环，为了判断每个输入的字符
            if (val[i].match(qjreg) != null) {//如果输入的字符是全角字符（=null说明是半角）则字符数为2
                hxc_len += 3;
            } else {//否的话为1
                hxc_len += 1;
            };
        };
        return hxc_len;//弹出该值
    };

    function max_content(val, max) {//函数：弹出input内容在规定长度下的值
        var return_val = "";//声明并赋值
        var count = 0;
        var qjreg = /[^\x00-\xff]/ig;
        for (i = 0; i < val.length; i++) {//与上面类似不赘述
            if (val[i].match(qjreg) != null) {
                count += 3;
            } else {
                count += 1;
            };
            if (count > max) {//当数字大约最大字符数时，跳出该循环
                break;
            };
            return_val += val[i];//值自增
        };
        return return_val;//弹出值
    };


}

function afterShow(){
    $j('#calendarTable td.currentTime').each(function (i,n) {
        $j(n).click(function () {
            $j('#calendarTable td.currentTime').removeClass('on');
            $j(this).addClass('on')
        });
    });
    $j('.delivery-hours span').each(function (i,n) {
        $j(n).click(function () {
            $j('.delivery-hours span').removeClass('on');
            $j(this).addClass('on')
        });
    });
}
function openDeliverytime(id,w,h,dateForm,dateTo,dateDiff){
    jQuery.fancybox({
        href:id,
        autoSize: false,
        fitToView: false,
        width: w+'px',
        height: h+'px',
        openEffect : 'fade',
        closeEffect : 'fade',
        centerOnScroll:false,
        openSpeed: 300,
        closeBtn: true,
        closeSpeed: 0,
        helpers: {
            overlay : {
                locked : false,
                opacity : 0.55,
                speedIn : 300,
                speedOut : 0
            }
        },
        beforeShow: function () {
            $j('.delivery-time').text('可选送货时间');
            var _td = $j('#calendar td');
            // var tdLenght = _td.length;

            _dateForm = new Date(dateForm).getTime();
            _dateTo = new Date(dateTo).getTime();


            $j("#calendar td").removeClass('currentTime')
            $j("#calendar td").removeClass('on')
            $j("#calendar td[data="+getDateStr(new Date(_dateForm))+"]").addClass('on')
            for(var time = _dateForm ; time <= _dateTo ; time += 24* 3600 * 1000){
                var _d = getDateStr(new Date(time))
                $j("#calendar td[data="+_d+"]").addClass('currentTime')
            }
        },
        afterShow:function () {
            afterShow();
            //顺丰 取当前选中的时间
            $j('.time-btn').click(function () {//显示默认时间
                var time = $j('#calendarTable td.currentTime.on').attr('data');
                var time1 = time.substr(5,1);
                var time2 = $j('#calendarTable td.currentTime.on span').text();
                var eq = $j('#calendarTable td.currentTime.on').index();
                var time3 = $j('tr.week th').eq(eq).text();
                var time4 = $j('.delivery-hours span.on').text();
                if(time4 == ''){
                    alert('请选中您配送的时间！');
                }else{
                    // _dateStr.substr(4,2)

                    $j('.delivery-time').text(time1+'月'+time2+'日'+'['+time3+']    '+time4);
                    //下一步按钮赋值
                    $j('.nextStepBtn.delivery-button').attr('delivery_main_time',$j('#calendarTable td.on').attr('data'));
                    $j('.nextStepBtn.delivery-button').attr('delivery_append_time',$j('.delivery-hours span.on').text());
                    $j.fancybox.close();
                }
            });
        },
        afterClose:function () {

        }
    });
}
function checkboxClickBlock() {
    var checkboxItems = $j(".jqTransformCheckbox"),
        checkboxLabel = $j(".checkboxLabel"),
        parentDataLength = $j('.parent');
    parentDataLength.each(function(index,elem){
        var DataLength = $j(elem).attr("data-length"),
            CheckedLength = $j(elem).find(".jqTransformChecked").length;
        if(DataLength == '0'){
            $j(this).find('li').addClass('MaskLayerShow');
        }else if(CheckedLength>=DataLength){
            if($j(elem).find(".jqTransformCheckbox:not(.jqTransformChecked)")){
                $j(this).find(".jqTransformCheckbox:not(.jqTransformChecked)").closest("li").addClass('MaskLayerShow');
                //$j(this).find('li').addClass('MaskLayerShow');
            }else{
                $j(this).closest("li").removeClass('MaskLayerShow');
            }
        }else if(DataLength < CheckedLength){
            $(elem).find(".jqTransformCheckbox:not(.jqTransformChecked)").closest("li").addClass('MaskLayerShow');
        }
    });
    checkboxItems.click(function () {
        var $this = $j(this),
            $parentTag = $this.closest(".parent"),
            maxLength = $parentTag.attr("data-length");
            maxLength = (maxLength == undefined) ? 1 : maxLength;
        if ($this.hasClass("jqTransformNoChecked")) {
            return false;
        }
        if ($this.hasClass("jqTransformChecked")) {
            $this.removeClass("jqTransformChecked");
            $parentTag.find(".jqTransformCheckbox").not(".jqTransformChecked").removeClass("jqTransformNoChecked");
            $parentTag.find(".jqTransformCheckbox:not(.jqTransformChecked)").closest("li").removeClass('MaskLayerShow');
            //$this.next().prop("checked", false);
            $this.next().click();
            if($this.next().attr('id') == 'statement-checkbox' && $this.closest('body').hasClass("checkout-onepage-index")){
                $this.parents('.statement-block').siblings('.statement-failed').fadeIn();
            }
            if($this.parent().parent().hasClass('billing-invoice-other-address')){
                $j('.modify_fapiao_address').val(0);
            }
            return false;
        }else {
            if($this.next().attr('id') == 'statement-checkbox' && $this.closest('body').hasClass("checkout-onepage-index")){
                $this.parents('.statement-block').siblings('.statement-failed').fadeOut();
            }
        }

        $this.addClass("jqTransformChecked");
        if($this.parent().parent().hasClass('billing-invoice-other-address')){
            $j('.modify_fapiao_address').val(1);
        }
        // if($j(this).closest('#update-cart-form').length>0){
        //     $parentTag.find(".jqTransformCheckbox:not(.jqTransformChecked)").addClass("jqTransformNoChecked");
        // }else {
            if (maxLength == $parentTag.find(".jqTransformChecked").length){
                $parentTag.find(".jqTransformCheckbox:not(.jqTransformChecked)").addClass("jqTransformNoChecked");
                $parentTag.find(".jqTransformCheckbox:not(.jqTransformChecked)").closest("li").addClass('MaskLayerShow');
                // $this.next().prop("checked", true);
            }

            // else {
                // $parentTag.find(".jqTransformCheckbox:not(.jqTransformChecked)").addClass("jqTransformNoChecked");
                // $this.next().click();
                // return false;
            // }
        // }
        //$this.next().attr("checked", true);
        $this.next().click();
    });
    checkboxLabel.click(function(){
        var $this = $j(this),
            $parentTag = $this.siblings(),
            $parent = $this.siblings().find("a");
        if ($parent.hasClass("jqTransformChecked")) {
            $parent.removeClass("jqTransformChecked");
            $parentTag.find(".jqTransformCheckbox").not(".jqTransformChecked").removeClass("jqTransformNoChecked");
            $parent.next().attr("checked", false);
            if($this.parent().hasClass('billing-invoice-other-address')){
                $j('.modify_fapiao_address').val(0);
            }
            $this.parents(".billing-invoice-click").siblings(".invoice-content").removeClass("invoice-content-show");
            $this.parents(".billing-invoice").siblings(".address-hide").removeClass("address-show");
            $this.parents(".jqtransformdone").siblings(".message-content").hide();
            return false;
        }
        $parent.addClass("jqTransformChecked");
        $parent.next().attr("checked", true);
        if($this.parent().hasClass('billing-invoice-other-address')){
            $j('.modify_fapiao_address').val(1);
        }
        $this.parents(".billing-invoice-click").siblings(".invoice-content").addClass("invoice-content-show");
        $this.parents(".billing-invoice").siblings(".address-hide").addClass("address-show");
        $this.parents(".jqtransformdone").siblings(".message-content").show();
    });
}

//checkbox checked
function checkboxBlock() {
    var timer = null;
    var checkboxBtn = $j('.checkboxBtn');
    checkboxBtn.click(function () {
        var t = $j(this);
        if(t.hasClass('show-statement-btn')){
            t.siblings('.show-statement-layer').css('display','block');
        }
        if(!t.hasClass('jqTransformNoChecked')){
            if(t.hasClass('jqTransformChecked')){
                t.removeClass('jqTransformChecked');
                t.next().click();
                if(t.hasClass('show-statement-btn')){
                    clearTimeout(timer);
                    timer = setTimeout(function() { //在单击事件中添加一个setTimeout()函数，设置单击事件触发的时间间隔
                        $j('.show-statement-layer').css('display','none');
                    }, 1000);
                }
            }else {
                t.addClass('jqTransformChecked');
                t.next().click();
                if(t.hasClass('show-statement-btn')){
                    clearTimeout(timer);
                    timer = setTimeout(function() { //在单击事件中添加一个setTimeout()函数，设置单击事件触发的时间间隔
                        $j('.show-statement-layer').css('display','none');
                    }, 1000);
                }
            }
        }else {
            return;
        }
    });
}
