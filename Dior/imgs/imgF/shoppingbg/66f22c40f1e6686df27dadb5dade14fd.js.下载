var target = "<?php echo $this->getUrl('customtext/index/edit/') ?>";
var remove = "<?php echo $this->getUrl('customtext/index/remove/') ?>";
function openFancybox(id,w,h,t){
    jQuery.fancybox({
        href:id,
        autoSize: false,
        fitToView: false,
        width: w+'px',
        height: h+'px',
        openEffect : 'fade',
        closeEffect : 'fade',
        centerOnScroll:false,
        openSpeed: 300,
        closeBtn: true,
        closeSpeed: 0,
        helpers: {
            overlay : {
                locked : false,
                opacity : 0.55,
                speedIn : 300,
                speedOut : 0
            }
        },
        beforeShow: function () {
            $j('.kezi-layer').val(t);
        }
    });
}
$j(function () {
    //情人节刻字
    $j('.kezi-Q').on('click',function () {
        var item_id = $j(this).attr('data-id');
        $j('#q-lettering-block').attr('item_id',item_id);
        openFancybox('#q-lettering-block','600','430');
    });

    $j('.kezi-back, .kezi-back-remove, .cnacel-btn').on('click',function () {
        $j.fancybox.close();
    });



});
var ajaxCartUpdateUrl = '/beauty/zh_cn/store/checkout/AjaxCart/update';
var cartListUrl = '/beauty/zh_cn/store/checkout/AjaxCart/list';
var deleteItemUrl = '/beauty/zh_cn/store/checkout/AjaxCart/delete';
var originalapiCartList = '/beauty/zh_cn/store/original_api/cart/list';
var swiperSampleC = $j('.swiper-sample');
var swiperTravelC = $j('.swiper-travel');
var swiperRibbionList = $j('.ribbion-list');
var swiperRibbionList2 = $j('.ribbion-list2');
var productListTracking = '';//list接口返回pcd选中商品
var TrackingCartCouponCode = '';//list接口返回pcd商品使用的CouponCode

function TrackingConfirmData(checkoutActin,ConfirmData) {
    var CheckoutConfirmData = ConfirmData;
    var coupon = $j('#coupon_code').val();
    if($j('.checkout-cart-index').length>0){
        if(coupon != ''){
            CheckoutConfirmData.CouponCode = coupon.split('成功使用')[1].replace(/\s*/g,"")
        }else {
            CheckoutConfirmData.CouponCode = ""
        }
    }
    trak.custom_event(checkoutActin, CheckoutConfirmData)
}
// 提交订单
function giftwarp($) {
    var btn = $('.btn-proceed-checkout');
    var continueCheckoutUrl =  btn.attr('data-target');
    btn.on('click', function(){
        var giftForm = $('#gwp-select-form');
        //GWD 追踪数据
        if(typeof _gsq != 'undefined') {
            _gsq.push(['GWD-005670', 'trackEvent', 'Shopping bag', 'Submit order', 'Shopping bag']);
        }
        if($j('.CheckoutConfirmData').text() != ''){
            if(productListTracking == ''){
                TrackingConfirmData('CheckoutConfirm',JSON.parse($j('.CheckoutConfirmData').text()));
            }else {
                var simpleProduct = productListTracking.simpleProduct;
                var bundleProduct = productListTracking.bundleProduct;
                var ProductArry = {"items":[],'CouponCode':''};
                var simpleProductArry = [];
                var bundleProductArry = [];
                //循环单品
                for (var i = 0;i<simpleProduct.length;i++) {
                    var name = simpleProduct[i].name;
                    var sku = simpleProduct[i].sku;
                    var quantity = simpleProduct[i].quantity;
                    var category = simpleProduct[i].fst_category;
                    var _price = simpleProduct[i].price;
                    var totalPrice = _price*quantity;
                    simpleProductArry.push({
                        '1st_category': category,
                        '2nd_category': "",
                        '3rd_category': "",
                        'brand': "PCD",
                        'color': "",
                        'name': name,
                        'qty': quantity,
                        'size': "",
                        'sku': sku,
                        'style': "",
                        'total_price': totalPrice+'.00',
                        'unit_price': _price+'.00'
                    });
                }
                //循环套装
                for (var k = 0;k<bundleProduct.length;k++) {
                    var children_item = bundleProduct[k].children_item;
                    for (var m = 0;m<children_item.length;m++) {
                        var name = children_item[m].name;
                        var sku = children_item[m].sku;
                        var quantity = bundleProduct[k].quantity;
                        var category = children_item[m].fst_category;
                        var _price = children_item[m].childPrice;
                        var totalPrice = _price*quantity;
                        bundleProductArry.push({
                            '1st_category': category,
                            '2nd_category': "",
                            '3rd_category': "",
                            'brand': "PCD",
                            'color': "",
                            'name': name,
                            'qty': quantity,
                            'size': "",
                            'sku': sku,
                            'style': "",
                            'total_price': totalPrice+'.00',
                            'unit_price': _price+'.00'
                        });
                    }
                }
                ProductArry.items = simpleProductArry.concat(bundleProductArry);
                ProductArry.CouponCode = TrackingCartCouponCode;
                TrackingConfirmData('CheckoutConfirm',ProductArry);
            }
        }
        var submitFnc = function(){
            if (giftForm.length > 0 && giftForm.is(':visible')) {
                if($j('#order-travel-size').css('display') == 'block' || $j('#sample-rule-gifts').css('display') == 'block'){
                    var hasValue = false;
                    giftForm.find('input:checkbox').each(function(index,item) {
                        if($(item).is(':checked')) {
                            hasValue = true;
                        }
                    });
                    if (hasValue) {
                        giftForm.submit(function(e){
                            $.ajax({
                                url: $(this).attr('action'),
                                type: "POST",
                                dataType: 'json',
                                data: giftForm.serialize(), // serializes the form's elements.
                                beforeSend:function(){
                                    $j('.updataCart').fadeIn();
                                },
                                success: function(data) {
                                    if (!data.Success) {
                                        $j('.updataCart').fadeOut();
                                        alert(Translator.translate(data.Message));
                                    } else {
                                        window.location = continueCheckoutUrl;
                                    }
                                }
                            });
                            e.preventDefault(); // avoid to execute the actual submit of the form.
                        });
                        giftForm.submit();
                    } else {
                        if(giftForm.find('input:checkbox').length>0){
                            window.location.reload();
                        }else {
                            window.location = continueCheckoutUrl;
                        }
                    }
                }else {
                    window.location = continueCheckoutUrl;
                }
            } else {
                $(".cart-please-wait").hide();
                window.location = continueCheckoutUrl
            }
        };
        submitFnc();
        return false;
    });
}

function swiperSlideClick() {
    // 小样默认选中第一个
    var swiperContainer = $j('.checkout-cart-index .swiper-container');
    swiperContainer.each(function (m,n) {
        var swiperList = $j(n).find('.swiper-slide');
        var swiperSlideOn = $j(n).find('.swiper-slide-on');
        var swiperListNum = swiperList.length;
        if(swiperListNum>0 && swiperSlideOn.length == 0){
            swiperList.eq('0').addClass('swiper-slide-on');
            swiperList.eq('0').find('input').addClass('checked').prop("checked", true);
        }
    });
    //ribbion list click
    $j('.ribbion-list .swiper-slide').on('click',function () {
        if(!$j(this).hasClass('swiper-slide-on')){
            $j(this).addClass('swiper-slide-on').siblings().removeClass('swiper-slide-on');
        }
    });
    $j('.ribbion-list2 .swiper-slide').on('click',function () {
        if(!$j(this).hasClass('swiper-slide-on')){
            $j(this).addClass('swiper-slide-on').siblings().removeClass('swiper-slide-on');
        }
    });
    $j('.swiper-slide').on('click',function () {
        if($j(this).parents('body').hasClass('checkout-cart-index')){
            if($j(this).hasClass('swiper-no-click')){
                return false;
            }else {
                if(!$j(this).hasClass('swiper-slide-on')){
                    $j(this).addClass('swiper-slide-on').siblings().removeClass('swiper-slide-on');
                    $j(this).find('input').addClass('checked').prop("checked", true);
                    $j(this).siblings().find('input').removeClass('checked').prop("checked", false);
                }
            }
        }
    });
}

function travel() {
    // travel切换
    var swiperTravel = new Swiper('.swiper-travel', {
        slidesPerView : 3,
        spaceBetween : 34,
        navigation: {
            nextEl: '.swiper-button-next1',
            prevEl: '.swiper-button-prev1'
        }
    });
}
function sample() {
    // sample切换
    var swiperSample = new Swiper('.swiper-sample', {
        slidesPerView : 3,
        spaceBetween : 34,
        navigation: {
            nextEl: '.swiper-button-next',
            prevEl: '.swiper-button-prev'
        }
    });
}
function swiperParametersBlock() {
    //pcd 热卖星品
    var swiperParameters = new Swiper('.parameters-swiper-travel', {
        slidesPerView : 2,
        spaceBetween : 20,
        navigation: {
            nextEl: '.parameters-button-next1',
            prevEl: '.parameters-button-prev1'
        },
        on: {
            slidePrevTransitionStart:function () {
                var parametersBlock =$j('.parameters-swiper-travel');
                var productsdatas = parametersBlock.find('.swiper-slide-active .parameters-btn span').text();
                var sku = this.activeIndex
                $j('#parameters').attr('data-sku',sku);
                gtmObj.event.slideLeftClick(productsdatas,'DiorCom_PCD_Checkout');
                openNum(sku,'parameters');
            },
            slideNextTransitionEnd:function () {
                var parametersBlock =$j('.parameters-swiper-travel');
                var productsdatas = parametersBlock.find('.swiper-slide-active .parameters-btn span').text();
                var sku = this.activeIndex
                $j('#parameters').attr('data-sku',sku);
                gtmObj.event.slideRightClick(productsdatas,'DiorCom_PCD_Checkout');
                openNum(sku,'parameters');
            }
        }
    });
}
function ParametersCDCBlock() {
    //cdc 热卖星品
    var ParametersCDCBlock = new Swiper('.parameters-CDCswiper-travel', {
        slidesPerView : 2,
        spaceBetween : 20,
        navigation: {
            nextEl: '.parameters-button-CDCnext',
            prevEl: '.parameters-button-CDCprev'
        },
        on: {
            slidePrevTransitionStart:function () {
                var parametersBlock =$j('.parameters-CDCswiper-travel');
                var productsdatas = parametersBlock.find('.swiper-slide-active .parameters-btn span').text();
                var sku = this.activeIndex;
                $j('#CDCparameters').attr('data-sku',sku);
                gtmObj.event.slideLeftClick(productsdatas,'DiorCom_CDC_Checkout');
               openNum(sku,'CDCparameters');
            },
            slideNextTransitionEnd:function () {
                var parametersBlock =$j('.parameters-CDCswiper-travel');
                var productsdatas = parametersBlock.find('.swiper-slide-active .parameters-btn span').text();
                var sku = this.activeIndex
                $j('#CDCparameters').attr('data-sku',sku);
                gtmObj.event.slideRightClick(productsdatas,'DiorCom_CDC_Checkout');
                openNum(sku,'CDCparameters');
            }
        }
    });
}
function swiperSlideInit() {
    $j('.swiper-slide').each(function (i,k) {
        if($j(k).hasClass('swiper-slide-on')){
            $j(this).siblings().addClass('swiper-slide-hide');
        }
    });
}
//留言卡切换
function cardItemsBlock ()
{
    var cardItems = $j('.cardItems');
    var _li = cardItems.find('li');
    var activeClass = cardItems.attr('data-sku');
    if(activeClass == ''){
        _li.removeClass('active');
        _li.eq(0).addClass('active');
        $j('.pcd-package .liuyan').show();
    }else {
        $j('.pcd-package .liuyan').hide();
        _li.each(function (i,n) {
            var s = $j(n).attr('data-sku');
            if(activeClass == s){
                _li.removeClass('active');
                $j(n).addClass('active')
            }
        });
    }
    _li.click(function () {
        if(!$j(this).hasClass('active')){
            var _sku = $j(this).attr('data-sku');
            $j(this).addClass('active');
            if ($j(this).parent().hasClass('card-special')) {
                $j(this).parent().siblings().removeClass('active');
            } else {
                if ($j(this).siblings().hasClass('card-special')) {
                    $j(this).siblings().removeClass('active');
                    $j(this).siblings().find('.cardJs').removeClass('active');
                } else {
                    $j(this).siblings().removeClass('active');
                }
            }
            if(_sku == ''){
                $j('.pcd-package .liuyan').show();
            }else {
                $j('.pcd-package .liuyan').hide();
            }
        }
    });
}
//购物车html更新
function updateHtml(data) {
    //产品list
    var productList = data.items; //pcd 商品
    productListTracking = data.items; //pcd 商品
    TrackingCartCouponCode = data.cartCouponCode; //pcd 商品coupon code
    var productList_CDC = data.cdc_items; //cdc 商品
    var cartSample = data.cart_simple;
    var cartTravel = data.cart_travel;
    var cartTotals = data.totalWithCurrency;
    var cartRule = data.lettering_rule_desc;
    var discountWithCurrency = data.discountWithCurrency;
    var cartSize = data.items.cartSize;
    var cartSizeCdc = data.cdc_items.cdc_cartSize;
    var cartNoItemsSize = data.items.cartNoItemsSize;
    var cartNoItemsSizeCdc = data.cdc_items.cdc_cartNoItemsSize;
    var cartSubTotalWithCurrency = data.cartSubTotalWithCurrency;
    var cartCouponCode = data.cartCouponCode;
    $j('.cart-totals-content').text(cartTotals);//购物车总价
    var cartTotalsBlock = data;
    var travel_items_count = data.cart_travel.travel_items_count;
    var next_items_count = data.cart_travel.next_items_count;
    var cart_simple = data.cart_simple;
    var haserror = data.hasError;
    var automatic = data;
    var cartemptyhtml ='<div class="cart-empty-bg">'+
                '<div class="page-title" style="margin-bottom: 0;">'+
                '<h1 style="float: none;">购物袋为空</h1>'+
                '</div>'+
                '<div class="cart-empty">'+
                '<div class="register-message-box">'+
               '</div>'+
                '<p>您的购物袋内没有物品</p>'+
                '<p>单击<a href="/beauty/zh_cn/home.html">这里</a>来继续购物</p>'+
                '</div>'+
                '</div>';
    if(haserror){
      window.location.reload();
    }
    $j('.cart-totals-wrapper .checkout-types').show();

    if(travel_items_count>0 || next_items_count>0){
        $j('#travel-rule-gifts').show();
    }else {
        $j('#travel-rule-gifts').hide();
    }
    if(cart_simple.size>0){
        $j('#sample-rule-gifts').show();
    }else {
        $j('#sample-rule-gifts').hide();
    }

    /* pcd和cdc 显示和隐藏内容*/
    if(cartSize == '0' && !cartNoItemsSize && cartSizeCdc == '0' && !cartNoItemsSizeCdc){
        $j('.shoppCartContent').html(cartemptyhtml);
    }else if(cartNoItemsSize && cartSize == '0' && cartNoItemsSizeCdc && cartSizeCdc == '0'){
        $j('.cart-totals-wrapper').css('display','none');
    }else if(cartSize != '0' || cartSizeCdc != '0'){
        $j('.cart-totals-wrapper').css('display','block');
    }

    if(cartSize == '0' && data.cdc_items.cdc_cartSize == '0'){
        $j('.cart-forms, #parameters, #gwp-select-form, .cart-totals-wrapper, .cart-footer').hide();
    }else if(cartSize == '0' && data.cdc_items.cdc_cartSize != '0'){
        $j('.cart-forms, #parameters, #gwp-select-form').hide();
        $j('.cart-totals-wrapper, .cart-footer, .cart-forms .coupon_display').show();
    }else if(cartSize != '0' && data.cdc_items.cdc_cartSize == '0'){
        $j('.cart-forms, .cart-forms .coupon_display, #parameters, #gwp-select-form, .cart-totals-wrapper, .cart-footer').show();
    }else {
        $j('.cart-forms, .cart-forms .coupon_display, #parameters, #gwp-select-form, .cart-totals-wrapper, .cart-footer').show();
    }

  /* cdc 显示和隐藏内容*/
  if(cartSizeCdc == '0' && !cartNoItemsSizeCdc){
    $j('#cdc-table').css('display','none');
  }else{
    $j('#cdc-table').css('display','block');
  }

    //判断购物车提示文案是否显示
    if(cartSize == '0' && !cartNoItemsSize){
        $j('.pc-notice').hide();
    }else {
        $j('.pc-notice').show();
    }
    //优惠券
    if(cartCouponCode == ''){
        var errorMsg = $j('.register-message-box .error-msg span').text();
        $j('#coupon_code').val("");
        $j('#coupon_code').attr('placeholder','输入您的活动码');
        $j('#coupon_code').removeAttr('readonly');
        $j('#coupon_code_btn').val("确定");
        $j('#remove-coupone').val('0');
        $j('#coupon_code_btn').find('span.btn_text').text("确定");
        $j('#coupon_code').removeClass('coupon-required-entry');
        $j('#advice-coupon-required-entry-coupon_code').css('opacity','0');
        if(errorMsg != '' && errorMsg.indexOf('优惠') != -1){
            $j('.register-message-box').html('');
        }
    }else {
        $j('#coupon_code').val("成功使用 "+cartCouponCode);
        $j('#coupon_code').attr('readonly','readonly');
        $j('#coupon_code_btn').val("取消使用");
        $j('#remove-coupone').val('1');
        $j('#coupon_code_btn').find('span.btn_text').text("取消使用");
    }

    if(cartTotalsBlock.cart_travel.status){
        var travelResult = template("travelTemplate",cartTravel);
        $j('#travel-rule-gifts').html(travelResult);
    }
    if(cartTotalsBlock.cart_simple.status) {
        var sampleResult = template("sampleTemplate", cartSample);
        $j('#sample-rule-gifts').html(sampleResult);
    }

    // PCD 购物车商品
    if( $j('#productList').length>0){
        var productListResult = template("productListTemplate",productList);
        $j('#productList').html(productListResult);
    }


    // CDC 购物车商品
    var productListResult_CDC = template("productListTemplateCdc", productList_CDC);
    $j('#productList2').html(productListResult_CDC);

    // pcd商品总价
    if($j('#cartTotalsBlock').length>0) {
        var cartTotalsBlockResult = template("cartTotalsBlockTemplate", cartTotalsBlock);
        $j('#cartTotalsBlock').html(cartTotalsBlockResult);
    }

    // cdc商品总价
    var cartTotalsBlockResultCdc = template("cartTotalsBlockTemplateCdc", cartTotalsBlock);
    $j('#cartTotalsBlockCdc').html(cartTotalsBlockResultCdc);

    //PCD 推荐商品 加购成功后 推荐下一个商品
    if($j('#parameters').length>0) {
        var parametersTemplateBlock = template("parametersTemplate", automatic);
        $j('#parameters').html(parametersTemplateBlock);
    }

    //CDC 推荐商品 加购成功后 推荐下一个商品
    if($j('#CDCparameters').length>0) {
        var parametersCDCTemplateBlock = template("parametersCDCTemplate", automatic);
        $j('#CDCparameters').html(parametersCDCTemplateBlock);
    }

    //PCD 推荐是否隐藏
    if(automatic.automatic.length>0){
        if(cartSize == '0') {
            $j('#parameters').hide();
        }else{
            $j('#parameters').show();
        }
    }else {
        $j('#parameters').hide();
    }

    //CDC 推荐是否隐藏
    if(automatic.cdc_automatic.length>0){
        $j('#CDCparameters').show();
    }else {
        $j('#CDCparameters').hide();
    }

    plusNum();
    minusNum();
    dropDown();
    // updateNum();
    travel();
    sample();
    swiperSlideClick();
    // swiperSlideInit();
    deleleKeZiBlock();
    showKeZi();
    editKeZi();
    checkbox($j);
    ParametersCDCBlock();
    swiperParametersBlock();
    // ajaxMiniCartList();
    //错误提示返回顶部
    // messageBbox();
    descriptionBlock();
}
//original_api minicat产品list
function ajaxMiniCartList() {
    $j.ajax({
        type: 'get',
        dataType: 'json',
        data:{"cart":true},
        url: originalapiCartList,
        beforeSend:function(){

        },
        success: function(data){

        }
    });
}
//购物车产品list
function ajaxCartList(action) {
    $j.ajax({
        type: 'POST',
        dataType: 'json',
        url: cartListUrl,
        beforeSend:function(){
            $j('.updataCart').fadeIn();
        },
        success: function(data){
            if(data.status){
              if($j('#cdc-table').length < 1 && data.cdc_items.cdc_cartSize > 0 || $j('#pcd-table').length < 1 && data.items.cartSize > 0){
                window.location.reload();
              }else{
                updateHtml(data);
                $j('.updataCart').fadeOut();
                if(action == 'delete'){
                  var pcdCartSize = data.items.cartSize;
                  var cartNoItemsSize = data.items.cartNoItemsSize;
                  var cdcCartSize = data.cdc_items.cdc_cartSize;
                  var cdc_cartNoItemsSize = data.cdc_items.cdc_cartNoItemsSize;
                  if(pcdCartSize == 0 && !cartNoItemsSize){
                    $j('#pcd-table').remove();
                  }
                  if(cdcCartSize == 0 && !cdc_cartNoItemsSize){
                    $j('#cdc-table').remove();
                  }
                }
              }
                if(data.is_show_discount){
                    $j('#cartTotalsBlockCdc tr:not(:last-child)').show();
                    $j('#productList2').css({'borderBottom':" none"});
                }else{
                    $j('#cartTotalsBlockCdc tr:not(:last-child)').hide();
                    $j('#productList2').css({'borderBottom':" 1px solid #C0C0C0"});
                }
            }else {
                $j('.updataCart').fadeOut();
                alert(data.message);
            }
        }
    });
}
//购物车增减数量post
function ajaxCartUpdate(data) {
    $j.ajax({
        type: 'POST',
        dataType: 'json',
        url: ajaxCartUpdateUrl,
        data:data,
        beforeSend:function(){
            $j('.updataCart').fadeIn();
        },
        success: function(data){
            if(data.status){
                ajaxCartList();
            }else {
                ajaxCartList();
                $j('.updataCart').fadeOut();
                alert(data.message);
                // ajaxCartList();
            }
        }
    });
}
//购物车数量选择
function plusNum() {
    var plusBtn = $j('.qty_box .plus');
    plusBtn.click(function () {
        var t =  $j(this);
        var text = t.siblings('.qty').text();
        var maxNum = $j(this).data('max-qty');
        if(!t.hasClass('disabled') && !t.hasClass('no-click')){
            var qty = t.siblings('span.qty');
            var i =  parseInt(qty.text());
            var itemId = t.siblings('input.qty').attr('data-item');
            qty.text();
            i++;
            if(i>=maxNum){
                if(qty.text() != maxNum){
                    var data={"item_id":itemId,"qty":i};
                    ajaxCartUpdate(data);
                    t.removeClass('disabled');
                }
                i=maxNum;
                qty.text(i);
                t.addClass('disabled');
            }else {
                t.removeClass('disabled');
                t.siblings('.minus').removeClass('disabled');
                qty.text(i);
                var data={"item_id":itemId,"qty":i};
                ajaxCartUpdate(data);
            }
        }else if(t.hasClass('no-click')){
            $j(this).siblings('.qty').text('1');
            alert('同一件刻字商品仅可购买1件');
        }
    });
}
//购物车数量选择
function minusNum() {
    var plusBtn = $j('.qty_box .minus');
    plusBtn.click(function () {
        var t =  $j(this);
        if(!t.hasClass('disabled')){
            var qty = t.siblings('span.qty');
            var i =  parseInt(qty.text());
            var itemId = t.siblings('input.qty').attr('data-item');
            qty.text();
            i--;
            if(i<=1){
                if(qty.text() != 1){
                    var data={"item_id":itemId,"qty":i};
                    ajaxCartUpdate(data);
                    t.removeClass('disabled');
                }
                i=1;
                qty.text(i);
                t.addClass('disabled');
                // t.siblings('.plus').removeClass('disabled');
            }else {
                t.removeClass('disabled');
                qty.text(i);
                var data={"item_id":itemId,"qty":i};
                ajaxCartUpdate(data);
            }
        }
    });
}
//下拉
function dropDown() {
    var dropdownItem =  $j('*[data="data-drop"]');
    dropdownItem.click(function () {
        if($j(this).hasClass('show-sub-menu')){
            $j(this).removeClass('show-sub-menu');
        }else {
            $j(this).addClass('show-sub-menu');
            if(dropdownItem.length>1){
                $j(this).parents('tr').siblings().find('.drop-down').removeClass('show-sub-menu');
            }
        }
        //onepage 修改当前地址 切换不同地址 赋值
        var region_id = $j('.checkout-onepage-index .recipient-address-text.city-default .shipping_region_id').attr('defaultvalue');
        var city_id = $j('.checkout-onepage-index .recipient-address-text.city-default .shipping_city_id').attr('defaultvalue');
        $j('.checkout-onepage-index .change-address').attr("data-regionid",region_id);
        $j('.checkout-onepage-index .change-address').attr("data-cityid",city_id);
    });
}

//商品是否删除弹窗
function popupDeleteItem() {
  $j(document).on('click','.cart-table .product-cart-remove .btn-remove',function (event) {
    event.preventDefault();
    var itemId = $j(this).attr('data-item');
    $j.fancybox({
      href: '#delete-product-popup',
      autoSize: false,
      fitToView: false,
      width: '600px',
      height: '240px',
      openEffect : 'fade',
      closeEffect : 'fade',
      centerOnScroll:false,
      openSpeed: 300,
      closeBtn: true,
      closeSpeed: 0,
      helpers: {
        overlay : {
          locked : false,
          opacity : 0.55,
          speedIn : 300,
          speedOut : 0
        }
      },
      beforeShow: function () {
        $j('#delete-product-popup .confirm-btn').attr('data-item',itemId);
      }
    });
  })
}

//购物车删除
function deleteItem() {
    $j('.delete-product-popup .confirm-btn').on('click',function () {
        var t = $j(this);
        var itemId = t.attr('data-item');
        $j.ajax({
            type: 'POST',
            dataType: 'json',
            url: deleteItemUrl,
            data:{'item_id':itemId},
            beforeSend:function(){
                $j.fancybox.close();
                $j('.updataCart').fadeIn();
            },
            success: function(data){
                if(data.status){
                    ajaxCartList('delete');
                }else {
                    $j('.updataCart').fadeOut();
                    alert(data.message);
                }
            }
        });
    });
}
function couponCode(){
    var btn = $j('#coupon_code_btn');
    btn.click(function () {
        var coupon_code_text = $j('#coupon_code');
        var text = coupon_code_text.val();
        var form = $j(this).parents('form#discount-coupon-form');
        var couponCodeUrl = form.attr('action');
        var removeCoupone = $j('#remove-coupone').val();
       if(text == ''){
           coupon_code_text.addClass('coupon-required-entry');
           $j('#advice-coupon-required-entry-coupon_code').animate({'opacity':'1'});
       }else {
           coupon_code_text.removeClass('coupon-required-entry');
           $j('#advice-coupon-required-entry-coupon_code').animate({'opacity':'0'});
           $j.ajax({
               type: 'post',
               url: couponCodeUrl,
               data:form.serialize(),
               beforeSend:function(){
                   btn.addClass('disabled');
                   btn.attr('disabled',true);
                   $j('.updataCart').fadeIn();
               },
               success: function(data){
                   btn.removeClass('disabled');
                   btn.removeAttr('disabled');
                   if(data.status){
                       if(removeCoupone == '0'){
                           gtmObj.event.applyCode(text,'apply_succeed');
                       }else if(removeCoupone == '1'){
                           var arr = text.split(" ");
                           var text1 = arr[1];
                           gtmObj.event.applyCode(text1,'apply_cancel');
                       }
                       if(btn.val() == '确定'){
                           $j('#coupon_code').val("成功使用 "+text);
                           $j('#coupon_code').attr('readonly','readonly');
                           btn.val("取消使用");
                           $j('#remove-coupone').val('1');
                           btn.find('span.btn_text').text("取消使用");
                       }else if(btn.val() == "取消使用"){
                          $j('#coupon_code').val("");
                          $j('#coupon_code').attr('placeholder','输入您的活动码');
                           $j('#coupon_code').removeAttr('readonly');
                           btn.val("确定");
                           $j('#remove-coupone').val('0');
                           btn.find('span.btn_text').text("确定");
                       }
                       ajaxCartList();
                       var errorMessage = '<ul class="messages"><li class="error-msg"><ul><li><span>'+data.message+'</span></li></ul></li></ul>';
                       $j('.register-message-box').html(errorMessage);
                       $j('.register-message-box').css('display','block');
                       //错误提示返回顶部
                       // messageBbox();
                   }else {
                       gtmObj.event.applyCode(text,'apply_failed');
                       var errorMessage = '<ul class="messages"><li class="error-msg"><ul><li><span>'+data.message+'</span></li></ul></li></ul>';
                       $j('.register-message-box').html(errorMessage);
                       $j('.register-message-box').css('display','block');
                       coupon_code_text.val('');
                       $j('.updataCart').fadeOut();
                       $j(window).scrollTop(0);
                       // ajaxCartList();
                       //错误提示返回顶部
                       // messageBbox();
                   }
               }
           });
       }
    });
}
// 刻字弹窗展示
function showKeZi() {
    $j('.kezi-w').on('click',function () {
        var maxLen = $j(this).attr('data-word');
        var item_id = $j(this).attr('data-id');
        var v_is = $j(this).siblings('.kezi-table').find('.kezi-v').text();
        var ta = $j(this).parents('.kezi').find('.kezi-v');
        //GWD 追踪数据
        var GWDName= '';
        var GWDSku = '';
        if($j(this).parent().hasClass('kezi-border-top')){
            //单品
            GWDName = $j(this).parents('tr').prev().find('.product-name').text();
            GWDSku = $j(this).parents('tr').prev().find('.plus').siblings('input').attr('data-cart-item-id');
        }else {
            //套装
            GWDName =$j(this).siblings('h3').text();
            GWDSku = $j(this).siblings('h3').attr('data-sku');
        }
        if(typeof _gsq != 'undefined') {
            _gsq.push(['GWD-005670', 'trackEvent', 'Shopping bag', 'Edit the lettering information', $j.trim(GWDName) + '_' + GWDSku]);
        }
        $j('#lettering-block').attr('GWDName',GWDName);
        $j('#lettering-block').attr('GWDSku',GWDSku);

        $j('#lettering-block .kezi-textarea').attr("maxlength",maxLen);
        $j('#lettering-block .kezi-content>span>b').text(maxLen);
        $j('#lettering-block').attr('item_id',item_id);
        if(ta.length){
            openFancybox('#lettering-block','600',$j('#lettering-block').attr('data-height'),v_is);
            $j(".kezi-textarea").trigger("input");
        }else {
            openFancybox('#lettering-block','600',$j('#lettering-block').attr('data-height'));
        }
    });
}
//编辑刻字弹窗
function editKeZi() {
    $j('.kezi-edit').on('click',function () {
        var maxLen = $j(this).attr('data-word');
        var item_id = $j(this).attr('data-id');
        var v_is = $j(this).parents('.kezi-list').find('.kezi-v').text();
        //GWD 追踪数据
      var GWDName = '';
      var GWDSku = '';
      if($j(this).parents('tr').hasClass('suit-block')){
        GWDName = $j(this).parents('.kezi-content-bg').siblings('.suit-name').text();
        GWDSku = $j(this).parents('.kezi-content-bg').siblings('.suit-name').attr('data-sku');
        }else {
          GWDName = $j(this).parents('tr').prev().find('.product-name').text();
          GWDSku = $j(this).parents('tr').prev().find('.plus').siblings('input').attr('data-cart-item-id');
        }
        $j('#lettering-block').attr('GWDName',GWDName);
        $j('#lettering-block').attr('GWDSku',GWDSku);

        $j('#lettering-block .kezi-textarea').attr("maxlength",maxLen);
        $j('#lettering-block .kezi-content>span>b').text(maxLen);
        $j('#lettering-block').attr('item_id',item_id);
        openFancybox('#lettering-block','600','270',v_is);
        $j(".kezi-textarea").trigger("input");
    });
}
//提交刻字信息ajax
function kezisubmitAjax(data) {
    $j.ajax({
        type: 'POST',
        dataType: 'json',
        url: kezitarget,
        data:data,
        beforeSend:function(){
            $j('.updataCart').fadeIn();
        },
        success: function(data){
            if(data.status == true){
                ajaxCartList();
            }else {
                $j('.updataCart').fadeOut();
                alert(data.message);
            }
        }
    });
}
//提交刻字信息
function kezisubmit() {
    //刻字弹窗提交
    $j('.kezi-tijiao').on('click',function () {
            var b = $j(this).parents('#lettering-block').find('.kezi-layer');
            var val = b.val();
            var vv = val.replace(/(^\s+)|(\s+$)/g,"").replace(/\s/g,"");
            var formKey = $j('input[name="form_key"]').val();

            var GWDName = $j.trim($j(this).parents('#lettering-block').attr('gwdname'));
            var GWDSku = $j(this).parents('#lettering-block').attr('gwdsku');

            if(val == ''){
                b.addClass('error');
            }else if(vv.length  == 0){
                b.addClass('error');
            }else {
                b.removeClass('error');
                $j.fancybox.close();
                var item_id = $j('#lettering-block').attr('item_id');
                var v =  $j('#lettering-block .kezi-layer').val();
                var data = {"item_id":item_id,"text":v,"form_key":formKey};
                kezisubmitAjax(data);
            }
        //GWD 追踪数据
        if(typeof _gsq != 'undefined') {
            _gsq.push(['GWD-005670', 'trackEvent', 'Shopping bag', 'Lettering_submit', GWDName + '_' + GWDSku, 0, {'1': v}]);
        }
    });
}
//刻字删除 弹窗展示
function deleleKeZiBlock() {
    $j('.kezi-remove-btn').on('click',function (event) {
        event.preventDefault();
        var item_id = $j(this).parents('tr.kezi-list').find('.kezi-btn-block').attr('data-id');
        $j('#remove-box-kezi').attr('data-id',item_id);
        openFancybox('#remove-box-kezi','600','240');
    });
}
//刻字删除ajax
function deleteAjax(data) {
    $j.ajax({
        type: 'POST',
        dataType: 'json',
        url: keziremove,
        data:data,
        beforeSend:function(){
            $j('.updataCart').fadeIn();
        },
        success: function(data){
            if(data.status == true){
                ajaxCartList();
            }else {
                $j('.updataCart').fadeOut();
                alert(data.message);
            }
        }
    });
}
//刻字删除
function deleteKeZi() {
    $j('.kezi-remove').on('click',function () {
        var item_id = $j('.remove-box-kezi').attr('data-id');
        var data = {"item_id":item_id};
        $j.fancybox.close();
        deleteAjax(data);
    });
}

//部分结算
function checkall($){
    var checkall = $('.checkall .checkallBlock');
    $(checkall).on('click',function () {
        var c = $j(this).parents('.checkall').next('.cart-table').find('td.checkbox');
        var thisC = $j(this).parents('.checkall').next('.cart-table').find('#checkall');
        var curtable = $j(this).parents('.checkall').next('.cart-table').attr('id');
        // thisC.click();
        // console.log(thisC.is(':checked'),"ui");
        if($(this).hasClass('active')){
            $(this).removeClass('active');
            thisC.next().click();
            $('.selected').val('');
            // thisC.prop("checked", false);
            c.each(function(k,p){
                var b = $(p).find("input[type='checkbox']");
                var pa = $(p).parents('#update-cart-form').find('.product-cart-actions');
                var s = pa.find('.btn');
                var v = b.val();
                $(p).find('.selected').val('');
                $(p).find('.unselected').val(v);
                $(p).find('.jqTransformCheckbox').removeClass('jqTransformChecked');
                b.click();
                // b.prop("checked", false);
                s.addClass('disabled');
            });
        }else {
            $(this).addClass('active');
            thisC.next().click();
            $j('.unselected').val('');
            c.each(function(k,p){
                var b = $(p).find("input[type='checkbox']");
                var pa = $(p).parents('#update-cart-form').find('.product-cart-actions');
                var s = pa.find('.btn');
                var v = b.val();
                $(p).find('.selected').val(v);
                $(p).find('.unselected').val('');
                $(p).find('.jqTransformCheckbox').addClass('jqTransformChecked');
                b.click();
                // b.prop("checked", true);
                s.removeClass('disabled');
            });
        }
        checkboxAjaxD($,curtable);
    });
}
function checkbox($){
    var c = $('.cart-table td.checkbox');
    var box = c.find('.jqTransformCheckbox');
    box.click(function(){
        var thisC = $(this).siblings('input');
        var vC = $(thisC).val();
        var p = $(this).parents('td.checkbox');
        var pa = $(this).parents('#update-cart-form').find('.product-cart-actions');
        var s = pa.find('.btn');
        var curtable = $(this).parents('.cart-table').attr('id');
        var checkall = $(this).parents('.cart-table').prev().find('.checkallBlock');
        thisC.click();
        if($(thisC).is(':checked')){
            $(this).addClass('jqTransformChecked');
            p.find('.selected').val(vC);
            p.find('.unselected').val('');
            var len = 0;
            $(this).parents('.cart-table').find('td.checkbox').each(function(){
                if($(this).find("input[type='checkbox']").is(':checked')){
                    len++;
                }
            });
            if(len == $(this).parents('.cart-table').find('td.checkbox').length){
                checkall.addClass('active');
                checkall.next('input').click();
            }
            s.removeClass('disabled');
        } else{
            $(this).removeClass('jqTransformChecked');
            p.find('.selected').val('');
            p.find('.unselected').val(vC);
            checkall.removeClass('active');
            checkall.prop("checked", false);
            s.addClass('disabled');
        }
        checkboxAjaxD($,curtable);
    });
}
function checkboxAjaxD($,curtable){
    var c = $('#'+curtable).find('td.checkbox');
    var s = c.find('.selected');
    var sn = s.length;
    var us = c.find('.unselected');
    var usn = us.length;
    var Tselected = '';
    var Tunselected = '';
    for(var i=0; i<sn; i++){
        var v = s.eq(i).val();
        if(v){
            Tselected += v + ',';
        }
    }
    for(var i=0; i<usn; i++){
        var v = us.eq(i).val();
        if(v){
            Tunselected += v + ',';
        }
    }
    var selected = Tselected.substring(0,Tselected.length-1);
    var unselected = Tunselected.substring(0,Tunselected.length-1);
    var data = {
        'selected':selected,
        'unselected':unselected
    };
    checkboxAjax(data,selected,$);
}
function checkboxAjax(d,s,$){
    var url = '/beauty/zh_cn/store/checkout/cart/ajaxPartSettlement';
    $.ajax({
        type:'post',
        url:url,
        data:d,
        dataType:'json',
        beforeSend:function(){
            $j('.updataCart').fadeIn();
        },
        success:function(data){
            if(data.success == 1){
                ajaxCartList();
            }else {
                $j('.updataCart').fadeOut();
                alert(data.error);
            }
        }
    });
}
function ribbionSwiperBlock() {
    // ribbion swipper 按钮
    if($j(swiperRibbionList).find('.swiper-slide').length>4){
        swiperRibbionList.parents('.swipper-block').find('.swiper-button-next2').show();
        swiperRibbionList.parents('.swipper-block').find('.swiper-button-prev2').show();
    }else {
        swiperRibbionList.parents('.swipper-block').find('.swiper-button-next2').hide();
        swiperRibbionList.parents('.swipper-block').find('.swiper-button-prev2').hide();
    }
    if($j(swiperRibbionList2).find('.swiper-slide').length>4){
        swiperRibbionList2.parents('.swipper-block').find('.swiper-button-next3').show();
        swiperRibbionList2.parents('.swipper-block').find('.swiper-button-prev3').show();
    }else {
        swiperRibbionList2.parents('.swipper-block').find('.swiper-button-next3').hide();
        swiperRibbionList2.parents('.swipper-block').find('.swiper-button-prev3').hide();
    }
    var swiperRibbion = new Swiper('.ribbion-list', {
        slidesPerView : 4,
        spaceBetween : 20,
        observer:true,
        observeParents:true,
        navigation: {
            nextEl: '.swiper-button-next2',
            prevEl: '.swiper-button-prev2'
        }
    });
    var swiperRibbion1 = new Swiper('.ribbion-list2', {
        slidesPerView : 4,
        spaceBetween : 20,
        observer:true,
        observeParents:true,
        navigation: {
            nextEl: '.swiper-button-next3',
            prevEl: '.swiper-button-prev3'
        }
    });
}
function isGiftChooseAjax(data) {
    var isGiftChooseUrl = '/beauty/zh_cn/store/checkout/AjaxCart/isGiftChoose';
    $j.ajax({
        type:'post',
        url:isGiftChooseUrl,
        data:data,
        dataType:'json',
        success:function(data){
        }
    });
}
function ribbion() {
    $j('.mcd-block ul.imgs li').eq('0').click();
    if($j('.pcd_ribbion').css('display') == 'block'){
        if($j('.ribbion-list').length>0 && $j('.ribbion-list').find('.swiper-slide-on').length == 0){
            $j('.ribbion-list .swiper-slide').eq('0').trigger('click');
            $j('.ribbion-list .swiper-slide').eq('0').addClass('swiper-slide-on');
            $j('.ribbion-list2 .swiper-slide').removeClass('swiper-slide-on');
        }
    }else if($j('.mcd_ribbion').css('display') == 'block'){
        if($j('.ribbion-list2').length>0 && $j('.ribbion-list2').find('.swiper-slide-on').length == 0){
            $j('.ribbion-list2 .swiper-slide').eq('0').trigger('click');
            $j('.ribbion-list2 .swiper-slide').eq('0').addClass('swiper-slide-on');
            $j('.ribbion-list .swiper-slide').removeClass('swiper-slide-on');
        }
    }
}
function giftBlock() {
    var gift_block = $j('.cart-gift-block .gift-title');
    ribbion();
    gift_block.on('click',function () {
        if($j(this).hasClass('active-on')){
            $j(this).removeClass('active-on');
            $j(this).parents('.cart-gift-block').removeClass('showGift');
            $j(this).parent().find('#priority-product-check').removeClass('disabled');
            if($j(this).data('type') == 'cdc'){
              var data = {"is_cdc_gift_choose":0};
            }else{
              var data = {"is_gift_choose":0};
            }
            ribbion();
            isGiftChooseAjax(data);
        }else {
            if($j(this).hasClass('disabled')){
                $j(this).removeClass('active-on');
            }else{
              $j(this).addClass('active-on');
              $j(this).parent().find('#priority-product-check').addClass('disabled');
              $j(this).parents('.cart-gift-block').addClass('showGift');
              if($j(this).data('type') == 'cdc'){
                var data = {"is_cdc_gift_choose":1};
              }else{
                var data = {"is_gift_choose":1};
              }
              isGiftChooseAjax(data);
              ribbionSwiperBlock();
            }
        }
    });
}
//商品是否需要马上发货checkbox
function checkPriorityPro () {
  var ele = $j('#priority-product-check');
  ele.on('click',function () {
    if($j(this).hasClass('active-on')){
        $j(this).removeClass('active-on');
        $j('.cdc-package .gift-title ').removeClass('disabled');
        $j('#unpacking-product').hide();
        $j('#all-product').show();
        $j('#package-num').html(parseInt($j('#package-num').text()) - 1);
    }else{
      if($j(this).hasClass('disabled')){
        $j(this).removeClass('active-on');
      }else{
        $j(this).addClass('active-on');
        $j('.cdc-package .gift-title').addClass('disabled');
        $j('#unpacking-product').show();
        $j('#all-product').hide();
        $j('#package-num').html(parseInt($j('#package-num').text()) + 1);
      }
    }
  });
}
//礼盒
function mcdBlock($){
    var b = $('.mcd-block>ul li');
    b.on('click',function () {
        var _this = $(this);
        var sku = _this.attr('data-id');
        if(!_this.hasClass('no-click')){
            ribbionSwiperBlock();
            if(!$(this).hasClass('active')){
                $(this).addClass('active');
                $(this).siblings('li').removeClass('active');
            }
            if(sku == 'PCD'){
                $('.pcd_ribbion').css('display','block');
                $('.mcd_ribbion').css('display','none');
                $('#package').prop("checked",true);
                $('#package1').prop("checked",false);
                if($j('.ribbion-list').length>0){
                    $j('.ribbion-list .swiper-slide').eq('0').trigger('click');
                    $j('.ribbion-list .swiper-slide').eq('0').addClass('swiper-slide-on');
                    $j('.ribbion-list2 .swiper-slide').removeClass('swiper-slide-on');
                }
            }else if(sku == 'MCD'){
                $('.pcd_ribbion').css('display','none');
                $('.mcd_ribbion').css('display','block');
                $('#package1').prop("checked",true);
                $('#package').prop("checked",false);
                if($j('.ribbion-list2').length>0){
                    $j('.ribbion-list2 .swiper-slide').eq('0').trigger('click');
                    $j('.ribbion-list2 .swiper-slide').eq('0').addClass('swiper-slide-on');
                    $j('.ribbion-list .swiper-slide').removeClass('swiper-slide-on');
                }
            }
        }
    });
}
var gtmObj = {
    eventName: 'trackEvent',
    event: {
        mcartClick: function ($productsdatas) {
            $productsdatas = JSON.parse($productsdatas);
            window.dataLayer.push({
                "event": "recommendToAddToCart",
                "AddToCartType": "Recommend Phase1",
                "ecommerce": {
                    "currencyCode": "CNY",
                    "add": {
                        "products": $productsdatas
                    }
                }
            })
        },
        detailClick: function ($productsdatas) {
            window.dataLayer.push({
                "event": "CartProductClick",
                "eventAction": "Click on a Product in a Cart",
                "eventCategory": "DiorCom_OneDior_Checkout",
                "ecommerce": {
                    "click": {
                        "actionField": {"list": "DiorCom_PCD_Checkout"},
                        "products": $productsdatas
                    }
                }
            })
        },
        cartButtonClick: function ($productsdatas,type) {
            //选颜色
            window.dataLayer.push({
                "event": "addToCart",
                "AddToCartType": "regular",
                "pageType":type,
                "ecommerce": {
                    "currencyCode": "CNY",
                    "add": {
                        "products": $productsdatas
                    }
                }
            })
        },
        RecommendButtonClick: function ($productsdatas,type) {
            //热卖新品 加购按钮
            window.dataLayer.push({
                "event": "addToCart",
                "AddToCartType": "Recommend",
                "pageType":type,
                "eventCategory":type,
                "eventAction": "add to cart_Recommend",
                "eventLabel": "RecommendCartButton",
                "ecommerce": {
                    "currencyCode": "CNY",
                    "add": {
                        "products": $productsdatas
                    }
                }
            })
        },
        slideLeftClick: function ($productsdatas,type) {
            $productsdatas = JSON.parse($productsdatas);
            window.dataLayer.push({
                "event": "trackEvent",
                "eventCategory":type,
                "eventAction":"product list view",
                "eventLabel": "RecommendSlideLeft"
            })
        },
        slideRightClick: function ($productsdatas,type) {
            $productsdatas = JSON.parse($productsdatas);
            window.dataLayer.push({
                "event": "trackEvent",
                "eventCategory":type,
                "eventAction":"product list view",
                "eventLabel": "RecommendSlideRight"
            })
        },
        applyCode: function (id,name) {
            window.dataLayer.push({
                "event": "trackEvent",
                "eventCategory":"DiorCom_PCD_Checkout",
                "eventAction":"product list view",
                "eventLabel": id+'|'+name
            })
        }
    }
};
function parametersBlock() {
    var itemsJson = {};
    var addUrl = '/beauty/zh_cn/store/ecapi/cart/add';//线上
    // var addUrl = '/beauty/zh_cn/store/original_api/cart/add';//本地
    $j(document).on('click','.parameters-block .parameters-list button',function () {
        var _this = $j(this);
        var pcdBlock = $j(_this).parents('#parameters').length;
        var cdcBlock = $j(_this).parents('#CDCparameters').length;
        if(!$j(this).hasClass('noAdd')){
            var sku = $j(this).parents('.parameters-list').attr('data-sku');
            var productsdatas = $j(this).siblings('span').text();
            var productName = JSON.parse(productsdatas);
            // productName.name = 'RecommendCartButton';
            $j(this).parents('#parameters').attr('data-sku', sku);
            $j(this).parents('#CDCparameters').attr('data-sku', sku);
            $j(this).addClass('noAdd');
            itemsJson['items'] = [{'sku': sku, 'quantity': 1,"is_auto":"1"}];
            //GWD 追踪数据
            var gwdName = $j(this).parents('.parameters-list').find('.name').text();
            if(typeof _gsq != 'undefined') {
                _gsq.push(['GWD-005670', 'trackEvent', 'Shopping bag', 'Best seller_add to the shopping bag', $j.trim(gwdName) + '_' + sku]);
            }

            $j.ajax({
                contentType: 'json',
                type: 'POST',
                url: addUrl,
                data:JSON.stringify(itemsJson),
                beforeSend:function(){
                    $j('.updataCart').fadeIn();
                },
                success: function(data){
                    // ajaxCartList();
                    $j.ajax({
                        type: 'POST',
                        dataType: 'json',
                        url: cartListUrl,
                        beforeSend:function(){
                            $j('.updataCart').fadeIn();
                        },
                        success: function(data){
                            if(data.status){
                                if(pcdBlock>0){
                                    //PCD 热卖新品加购
                                    gtmObj.event.RecommendButtonClick(productName,"DiorCom_PCD_Checkout");
                                }else if(cdcBlock>0){
                                    //CDC 热卖新品加购
                                    gtmObj.event.RecommendButtonClick(productName,"DiorCom_CDC_Checkout");
                                }
                                updateHtml(data);
                                var skuItemsPcd = data.automatic;
                                var skuItemsCdc = data.cdc_automatic;
                                //PCD 会触发
                                if(pcdBlock>0){
                                    clickNum(skuItemsPcd.length,'parameters');
                                }
                                //CDC 会触发
                                if(cdcBlock){
                                    clickNum(skuItemsCdc.length,'CDCparameters');
                                }

                                $j('.updataCart').fadeOut();
                                if(data.is_show_discount){
                                    $j('#cartTotalsBlockCdc tr:not(:last-child)').show();
                                    $j('#productList2').css({'borderBottom':" none"});
                                }else{
                                    $j('#cartTotalsBlockCdc tr:not(:last-child)').hide();
                                    $j('#productList2').css({'borderBottom':" 1px solid #C0C0C0"});
                                }
                            }else {
                                $j('.updataCart').fadeOut();
                                alert(data.message);
                            }
                        }
                    });

                }
            });
        }
    });
}

function ajaxBlock(url, data) {
    $j.ajax({
        url: url,
        type: 'POST',
        data:JSON.stringify(data),
        contentType: 'json',
        success: function (resp) {
        }
    });
}
function openNum(sku,id){
    var data = { "open_num": [] };
    var _id = $j('#'+id);
    var openUrl =$j(_id).attr('data-url');
    var list = $j(_id).find('.parameters-list');
    if(sku != 'undefined'){
        var s = Number($j(_id).attr('data-sku'));
        list.each(function (m,n) {
            console.log(m,n,'ppp')
            if(m > s-1 && m <= s+1){
                var skus =  $j(n).attr('data-sku');
                data.open_num.push({"sku":skus});
            }
        });
    }else {
        list.each(function (m,n) {
            console.log(m,n)
            if(m<=1){
                var sku =  $j(n).attr('data-sku');
                data.open_num.push({"sku":sku});
            }
        });
    }
    ajaxBlock(openUrl,data);

}
function clickNum(k,t){
        var parameterBlock = $j('#'+t);
        var opendata = [];
        var clickNum = [];
        var openUrl = $j(parameterBlock).attr('data-url');
        var list = $j(parameterBlock).find('.parameters-list').attr('data-sku');
        var click_num = $j(parameterBlock).attr('data-sku');
        // clickNum[0] = click_num;
        clickNum.push({'sku':click_num});
        $j(parameterBlock).find('.parameters-list').each(function (m,n) {
            if(m<=1) {
                var sku = $j(n).attr('data-sku');
                opendata.push({'sku': sku});
            }
            // opendata[m]={"sku":sku}
        });
        var data = {};
        if(k<2){
           data = {"click_num":clickNum};
        }else {
            data = {"click_num":clickNum,"open_num":opendata};
        }
    ajaxBlock(openUrl,data);
}
//错误提示返回顶部
function messageBbox() {
    if($j(document).find('.register-message-box .messages').length>0){
        $j(document).find('body,html').animate({
            scrollTop: 0
        },1000);
        return false;
    }
}
//购物车埋点
function checkoutGtm() {
    $j(document).on('click','#productList .product-cart-image',function () {
        var _href = $j(this).find('a').attr('href');
        if(_href != "javascript:void(0);"){
            var sku = $j(this).parents('tr').find('.product-cart-actions input').attr('data-cart-item-id')
            var product = [{
                "id": sku,
                "category": "",
                "price": "",
                "name": "",
                "brand": "",
                "variant": "",
                "dimension25": "",
                "dimension16": "",
                "position": "1"
            }];
            gtmObj.event.detailClick(product);
        }
    });
}

function checkCouponCode() {
    //页面load 判断优惠码DIORGIFT是否存在
    var url = window.location.href;
    var arr = url.split("?");
    var thisNameAll = arr[1];
    if(thisNameAll == 'DIORGIFT' || thisNameAll == 'NEW'){
        //通过link截取 判断
        $j('#remove-coupone').val('0');
        $j('#coupon_code_btn>span>span').text('确定');
        $j('#coupon_code').val(thisNameAll);
        $j('#coupon_code_btn').click();
    }
}

function descriptionBlock() {
//购物车选色号
    if($j('.checkout-cart-index').length>0 && $j('.getDescription li').length>1){
        $j('.desc-box').click(function (event) {
            event.stopPropagation();
            var items = $j(this).siblings('.getDescription').find('li').length;
            if(items>1){
                var productsdatas = $j(this).siblings('.getDescription').find('li.active span.gtm').text();
                var productName = JSON.parse(productsdatas);
                productName.name = 'CheckoutShadeChange';
                gtmObj.event.cartButtonClick(productName,'DiorCom_PCD_Checkout');
                if($j(this).parents('.product-cart-sku-desc').hasClass('showDes1')){
                    $j(this).parent('.product-cart-sku-desc').removeClass('showDes1');
                }else {
                    $j(this).parent('.product-cart-sku-desc').addClass('showDes1');
                    $j(this).parents('tr').siblings().find('.product-cart-sku-desc').removeClass('showDes1');
                }
            }
        });
        $j('body').click(function () {
            $j('.product-cart-sku-desc').each(function (i,m) {
                if($j(m).hasClass('showDes')){
                    $j(m).removeClass('showDes');
                }else if($j(m).hasClass('showDes1')){
                    $j(m).removeClass('showDes1');
                }
            });
        });
        $j('.getDescription').niceScroll({
            cursorcolor:"#ccc",
            cursorborderradius: "2px", // 滚动条圆角（像素）
            autohidemode: false,
            cursorwidth: '4px',
            railpadding:{'right':2,'top':2},
            zindex:10
        });
        $j('.getDescription li').click(function () {
            var _t = $j(this);
            var _eq = _t.parents('ul.getDescription').find('li.active').index();
            if(!$j(this).hasClass('active')){
                var text = $j(this).find('span.colorSwatch').text();
                var oldText =  $j(this).parent('.getDescription').siblings('.desc-box').text();
                var sku = $j(this).attr('data-sku');
                var id = $j(this).parents('tr').find('.product-cart-remove a').attr('data-item');
                var changeProductToQuote = '/beauty/zh_cn/store/checkout/AjaxCart/changeProductToQuote';
                var productsdatas = $j(this).find('span.gtm').text();
                var productName = JSON.parse(productsdatas);
                productName.name = 'CheckoutShadeChanged';
                gtmObj.event.cartButtonClick(productName,'DiorCom_PCD_Checkout');
                $j(this).addClass('active');
                $j(this).siblings().removeClass('active');
                $j(this).parents('ul.getDescription').siblings('.desc-box').text(text);
                $j(this).parents('.product-cart-sku-desc').removeClass('showDes');
                $j(this).parents('.product-cart-sku-desc').removeClass('showDes1');
                $j.ajax({
                    type: 'POST',
                    dataType: 'json',
                    url: changeProductToQuote,
                    data:{'sku':sku,'item_id':id},
                    beforeSend:function(){
                        $j('.updataCart').fadeIn();
                    },
                    success: function(data){
                        if(data.status){
                            ajaxCartList();
                        }else {
                            $j('.updataCart').fadeOut();
                            _t.parents('ul.getDescription').find('li').removeClass('active');
                            _t.parents('ul.getDescription').find('li').eq(_eq).addClass('active');
                            $j(_t).parents('.getDescription').siblings('.desc-box').text(oldText);
                            alert(data.message)
                        }
                    }
                });
            }
        });
    }
}

//购物车优惠券弹窗
function conponLayer(){
    $j('.coupon-tooltip-btn').click(function () {
        var couponLayer = $j('#coupon-layer');
        jQuery.fancybox({
            href:'#coupon-layer',
            autoSize: false,
            fitToView: false,
            width: $j(couponLayer).attr('data-width')+'px',
            height: $j(couponLayer).attr('data-height')+'px',
            openEffect : 'fade',
            closeEffect : 'fade',
            centerOnScroll:false,
            openSpeed: 300,
            closeBtn: true,
            closeSpeed: 0,
            padding:0,
            helpers: {
                overlay : {
                    locked : false,
                    opacity : 0.55,
                    speedIn : 300,
                    speedOut : 0
                }
            }
        });
    });
}

$j(function () {

    conponLayer();

    // 继续购物
    $j('.continueShoppingButton').click(function () {
        //GWD 追踪数据
        if(typeof _gsq != 'undefined') {
            _gsq.push(['GWD-005670', 'trackEvent', 'Shopping bag', 'Keep Shopping', 'Shopping bag']);
        }
    });

    deleteItem();
    popupDeleteItem();
    travel();
    sample();
    // swiperSlideInit();
    swiperSlideClick();
    plusNum();
    minusNum();
    // dropDown();
    // updateNum();

    couponCode();
    checkCouponCode();
    showKeZi();
    editKeZi();
    kezisubmit();
    deleteKeZi();
    deleleKeZiBlock();
    giftwarp($j);
    checkall($j);
    checkbox($j);
    giftBlock();
    checkPriorityPro();
    ribbionSwiperBlock();
    mcdBlock($j);
    parametersBlock();
    checkoutGtm();
    cardItemsBlock();
    //beigin 判断CDC PCD是否都存在热卖新品
    var isAllparameters;
    if($j('#CDCparameters').css('display') == 'block' && $j('#parameters').css('display') == 'block'){
        openNum('undefined','CDCparameters');
        openNum('undefined','parameters');
    }
    if($j('#CDCparameters').css('display') == 'block' && $j('#parameters').css('display') == 'none'){
        openNum('undefined','CDCparameters');
    }
    if($j('#CDCparameters').css('display') == 'none' && $j('#parameters').css('display') == 'block'){
        openNum('undefined','parameters');
    }
    //end 判断CDC PCD是否都存在热卖新品
    $j(document).on('keypress','#coupon_code',function (event) {
        if(event.keyCode ==13){
            event.preventDefault();
            $j('#coupon_code_btn').trigger("click");
        }
    });
    //错误提示返回顶部
    // messageBbox();
    // $j('#checkout-payment-method-load input').uniform();
    descriptionBlock();
    ParametersCDCBlock();
    swiperParametersBlock();
});

